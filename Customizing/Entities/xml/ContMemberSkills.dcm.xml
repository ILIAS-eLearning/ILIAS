<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ContMemberSkills" table="cont_member_skills">
    <id name="objId" type="integer" column="obj_id"/>
    <id name="userId" type="integer" column="user_id"/>
    <id name="trefId" type="integer" column="tref_id"/>
    <id name="skillId" type="integer" column="skill_id"/>
    <field name="levelId" type="integer" column="level_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="published" type="boolean" column="published" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
