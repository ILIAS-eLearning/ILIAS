<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FrmPostsTree" table="frm_posts_tree">
    <indexes>
      <index name="i1_idx" columns="thr_fk"/>
      <index name="i3_idx" columns="parent_pos"/>
      <index name="i2_idx" columns="pos_fk"/>
    </indexes>
    <id name="fptPk" type="bigint" column="fpt_pk">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="thrFk" type="bigint" column="thr_fk" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="posFk" type="bigint" column="pos_fk" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="parentPos" type="bigint" column="parent_pos" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lft" type="integer" column="lft" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="rgt" type="integer" column="rgt" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="depth" type="integer" column="depth" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fptDate" type="datetime" column="fpt_date" nullable="true"/>
  </entity>
</doctrine-mapping>
