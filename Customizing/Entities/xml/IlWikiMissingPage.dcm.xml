<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlWikiMissingPage" table="il_wiki_missing_page">
    <indexes>
      <index name="i1_idx" columns="wiki_id,target_name"/>
    </indexes>
    <id name="wikiId" type="integer" column="wiki_id"/>
    <id name="sourceId" type="integer" column="source_id"/>
    <id name="targetName" type="string" column="target_name" length="200"/>
  </entity>
</doctrine-mapping>
