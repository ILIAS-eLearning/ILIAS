<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplQstType" table="qpl_qst_type">
    <id name="questionTypeId" type="integer" column="question_type_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="typeTag" type="string" column="type_tag" length="35" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="plugin" type="boolean" column="plugin" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pluginName" type="string" column="plugin_name" length="40" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
