<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WfeWorkflows" table="wfe_workflows">
    <id name="workflowId" type="integer" column="workflow_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="workflowType" type="string" column="workflow_type" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="workflowContent" type="string" column="workflow_content" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="workflowClass" type="string" column="workflow_class" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="workflowLocation" type="string" column="workflow_location" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="subjectType" type="string" column="subject_type" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="subjectId" type="integer" column="subject_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="contextType" type="string" column="context_type" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contextId" type="integer" column="context_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="workflowInstance" type="text" column="workflow_instance" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="active" type="integer" column="active" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
