<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FileData" table="file_data">
    <id name="fileId" type="integer" column="file_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fileName" type="string" column="file_name" length="250" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="fileType" type="string" column="file_type" length="250" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="fileSize" type="integer" column="file_size" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="version" type="integer" column="version" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fMode" type="string" column="f_mode" length="8" nullable="true">
      <options>
        <option name="fixed">1</option>
        <option name="default">object</option>
      </options>
    </field>
    <field name="rating" type="boolean" column="rating" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pageCount" type="bigint" column="page_count" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="maxVersion" type="integer" column="max_version" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
