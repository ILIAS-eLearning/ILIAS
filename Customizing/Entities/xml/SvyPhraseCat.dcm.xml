<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SvyPhraseCat" table="svy_phrase_cat">
    <indexes>
      <index name="i1_idx" columns="phrase_fi"/>
      <index name="i2_idx" columns="category_fi"/>
    </indexes>
    <id name="phraseCategoryId" type="integer" column="phrase_category_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="phraseFi" type="integer" column="phrase_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="categoryFi" type="integer" column="category_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="sequence" type="integer" column="sequence" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="other" type="smallint" column="other" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="scale" type="integer" column="scale" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
