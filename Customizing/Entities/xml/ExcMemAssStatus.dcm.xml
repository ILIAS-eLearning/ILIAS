<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ExcMemAssStatus" table="exc_mem_ass_status">
    <id name="assId" type="integer" column="ass_id"/>
    <id name="usrId" type="integer" column="usr_id"/>
    <field name="notice" type="string" column="notice" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="returned" type="boolean" column="returned" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="solved" type="boolean" column="solved" nullable="true"/>
    <field name="statusTime" type="datetime" column="status_time" nullable="true"/>
    <field name="sent" type="boolean" column="sent" nullable="true"/>
    <field name="sentTime" type="datetime" column="sent_time" nullable="true"/>
    <field name="feedbackTime" type="datetime" column="feedback_time" nullable="true"/>
    <field name="feedback" type="boolean" column="feedback" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="status" type="string" column="status" length="9" nullable="true">
      <options>
        <option name="fixed">1</option>
        <option name="default">notgraded</option>
      </options>
    </field>
    <field name="mark" type="string" column="mark" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="uComment" type="string" column="u_comment" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
