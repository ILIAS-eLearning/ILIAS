<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SvyAnonymous" table="svy_anonymous">
    <indexes>
      <index name="i1_idx" columns="survey_key,survey_fi"/>
      <index name="i2_idx" columns="survey_fi"/>
      <index name="i3_idx" columns="sent"/>
    </indexes>
    <id name="anonymousId" type="integer" column="anonymous_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="surveyKey" type="string" column="survey_key" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="surveyFi" type="integer" column="survey_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="userKey" type="string" column="user_key" length="40" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="externaldata" type="string" column="externaldata" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sent" type="smallint" column="sent" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
