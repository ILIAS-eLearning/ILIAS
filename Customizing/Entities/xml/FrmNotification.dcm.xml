<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FrmNotification" table="frm_notification">
    <indexes>
      <index name="i1_idx" columns="user_id,thread_id"/>
    </indexes>
    <id name="notificationId" type="integer" column="notification_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="frmId" type="integer" column="frm_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="threadId" type="integer" column="thread_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="adminForceNoti" type="boolean" column="admin_force_noti" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="userToggleNoti" type="boolean" column="user_toggle_noti" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="userIdNoti" type="integer" column="user_id_noti" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
