<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplQuestions" table="qpl_questions">
    <indexes>
      <index name="i1_idx" columns="question_type_fi"/>
      <index name="i3_idx" columns="obj_fi"/>
      <index name="i4_idx" columns="title"/>
      <index name="i2_idx" columns="original_id"/>
    </indexes>
    <id name="questionId" type="integer" column="question_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="questionTypeFi" type="integer" column="question_type_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="objFi" type="integer" column="obj_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="author" type="string" column="author" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="owner" type="integer" column="owner" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="workingTime" type="string" column="working_time" length="8" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">00:00:00</option>
      </options>
    </field>
    <field name="points" type="float" column="points" precision="10" scale="0" nullable="true"/>
    <field name="complete" type="string" column="complete" length="1" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="originalId" type="integer" column="original_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="created" type="integer" column="created" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="nrOfTries" type="integer" column="nr_of_tries" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="questionText" type="text" column="question_text" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="addContEditMode" type="string" column="add_cont_edit_mode" length="16" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="externalId" type="string" column="external_id" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lifecycle" type="string" column="lifecycle" length="16" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">draft</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
