<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplAKprim" table="qpl_a_kprim">
    <indexes>
      <index name="i1_idx" columns="question_fi"/>
    </indexes>
    <id name="questionFi" type="integer" column="question_fi"/>
    <id name="position" type="integer" column="position"/>
    <field name="answertext" type="string" column="answertext" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="imagefile" type="string" column="imagefile" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="correctness" type="boolean" column="correctness" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
