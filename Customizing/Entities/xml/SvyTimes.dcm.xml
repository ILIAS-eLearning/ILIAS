<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SvyTimes" table="svy_times">
    <indexes>
      <index name="i1_idx" columns="finished_fi"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="finishedFi" type="integer" column="finished_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="enteredPage" type="integer" column="entered_page" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="leftPage" type="integer" column="left_page" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="firstQuestion" type="integer" column="first_question" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
