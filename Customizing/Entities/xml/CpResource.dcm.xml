<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CpResource" table="cp_resource">
    <indexes>
      <index name="i1_idx" columns="id"/>
    </indexes>
    <id name="cpNodeId" type="integer" column="cp_node_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="base" type="string" column="base" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="href" type="string" column="href" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="id" type="string" column="id" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="scormtype" type="string" column="scormtype" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cType" type="string" column="c_type" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
