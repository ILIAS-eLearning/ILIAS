<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlQplQstFqUnit" table="il_qpl_qst_fq_unit">
    <indexes>
      <index name="i2_idx" columns="question_fi"/>
    </indexes>
    <id name="unitId" type="integer" column="unit_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="unit" type="string" column="unit" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="factor" type="float" column="factor" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="baseunitFi" type="integer" column="baseunit_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="categoryFi" type="integer" column="category_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="sequence" type="integer" column="sequence" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="questionFi" type="integer" column="question_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
