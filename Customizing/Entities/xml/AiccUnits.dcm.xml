<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AiccUnits" table="aicc_units">
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cType" type="string" column="c_type" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="commandLine" type="string" column="command_line" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="maxTimeAllowed" type="time" column="max_time_allowed" nullable="true"/>
    <field name="timeLimitAction" type="string" column="time_limit_action" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="maxScore" type="float" column="max_score" precision="10" scale="0" nullable="true"/>
    <field name="coreVendor" type="string" column="core_vendor" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="systemVendor" type="string" column="system_vendor" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fileName" type="string" column="file_name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="masteryScore" type="integer" column="mastery_score" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="webLaunch" type="string" column="web_launch" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="auPassword" type="string" column="au_password" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
