<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CpCondition" table="cp_condition">
    <id name="cpNodeId" type="integer" column="cp_node_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cCondition" type="string" column="c_condition" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="measurethreshold" type="string" column="measurethreshold" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cOperator" type="string" column="c_operator" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="referencedobjective" type="string" column="referencedobjective" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
