<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RegAccessLimit" table="reg_access_limit">
    <id name="roleId" type="integer" column="role_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="limitAbsolute" type="integer" column="limit_absolute" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="limitRelativeD" type="integer" column="limit_relative_d" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="limitRelativeM" type="integer" column="limit_relative_m" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="limitRelativeY" type="integer" column="limit_relative_y" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="limitMode" type="string" column="limit_mode" length="16" nullable="true">
      <options>
        <option name="fixed">1</option>
        <option name="default">absolute</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
