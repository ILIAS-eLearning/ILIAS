<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EventFile" table="event_file">
    <id name="fileId" type="integer" column="file_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="eventId" type="integer" column="event_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fileName" type="string" column="file_name" length="64" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="fileType" type="string" column="file_type" length="64" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="fileSize" type="integer" column="file_size" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
