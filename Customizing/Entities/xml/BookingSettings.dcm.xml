<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BookingSettings" table="booking_settings">
    <id name="bookingPoolId" type="integer" column="booking_pool_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="publicLog" type="boolean" column="public_log" nullable="true"/>
    <field name="poolOffline" type="boolean" column="pool_offline" nullable="true"/>
    <field name="slotsNo" type="smallint" column="slots_no" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="scheduleType" type="boolean" column="schedule_type" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="ovlimit" type="boolean" column="ovlimit" nullable="true"/>
    <field name="rsvFilterPeriod" type="smallint" column="rsv_filter_period" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="reminderStatus" type="boolean" column="reminder_status" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="reminderDay" type="integer" column="reminder_day" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lastRemindTs" type="integer" column="last_remind_ts" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
