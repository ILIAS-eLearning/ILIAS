<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GlossaryTerm" table="glossary_term">
    <indexes>
      <index name="i1_idx" columns="glo_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="gloId" type="integer" column="glo_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="term" type="string" column="term" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="language" type="string" column="language" length="2" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="importId" type="string" column="import_id" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createDate" type="datetime" column="create_date" nullable="true"/>
    <field name="lastUpdate" type="datetime" column="last_update" nullable="true"/>
  </entity>
</doctrine-mapping>
