<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdmSetTemplValue" table="adm_set_templ_value">
    <id name="templateId" type="integer" column="template_id"/>
    <id name="setting" type="string" column="setting" length="40"/>
    <field name="value" type="string" column="value" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hide" type="boolean" column="hide" nullable="true"/>
  </entity>
</doctrine-mapping>
