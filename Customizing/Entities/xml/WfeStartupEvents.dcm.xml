<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WfeStartupEvents" table="wfe_startup_events">
    <id name="eventId" type="integer" column="event_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="workflowId" type="string" column="workflow_id" length="60" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="content" type="string" column="content" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="subjectType" type="string" column="subject_type" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="subjectId" type="integer" column="subject_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="contextType" type="string" column="context_type" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contextId" type="integer" column="context_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
