<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LdapRoleAssignments" table="ldap_role_assignments">
    <id name="serverId" type="integer" column="server_id"/>
    <id name="ruleId" type="boolean" column="rule_id"/>
    <field name="type" type="boolean" column="type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="dn" type="string" column="dn" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="attribute" type="string" column="attribute" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isdn" type="boolean" column="isdn" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="attName" type="string" column="att_name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="attValue" type="string" column="att_value" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="roleId" type="integer" column="role_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="addOnUpdate" type="boolean" column="add_on_update" nullable="true"/>
    <field name="removeOnUpdate" type="boolean" column="remove_on_update" nullable="true"/>
    <field name="pluginId" type="integer" column="plugin_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
