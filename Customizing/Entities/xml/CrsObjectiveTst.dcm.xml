    </id>
    <field name="objectiveId" type="integer" column="objective_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="refId" type="integer" column="ref_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="objId" type="integer" column="obj_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tstStatus" type="boolean" column="tst_status" nullable="true"/>
    <field name="tstLimit" type="boolean" column="tst_limit" nullable="true"/>
    <field name="tstLimitP" type="smallint" column="tst_limit_p" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
