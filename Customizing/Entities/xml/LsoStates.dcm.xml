<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LsoStates" table="lso_states">
    <id name="lsoRefId" type="integer" column="lso_ref_id"/>
    <id name="usrId" type="integer" column="usr_id"/>
    <field name="currentItem" type="integer" column="current_item" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="states" type="text" column="states" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstAccess" type="string" column="first_access" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastAccess" type="string" column="last_access" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
