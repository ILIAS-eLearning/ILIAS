<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CpOrganization" table="cp_organization">
    <indexes>
      <index name="i1_idx" columns="id"/>
      <index name="i2_idx" columns="sequencingid"/>
    </indexes>
    <id name="cpNodeId" type="integer" column="cp_node_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="id" type="string" column="id" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="objectivesglobtosys" type="boolean" column="objectivesglobtosys" nullable="true"/>
    <field name="sequencingid" type="string" column="sequencingid" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="structure" type="string" column="structure" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
