<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlCertCronQueue" table="il_cert_cron_queue">
    <indexes>
      <index name="i1_idx" columns="obj_id,usr_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="objId" type="integer" column="obj_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="usrId" type="integer" column="usr_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="adapterClass" type="string" column="adapter_class" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="state" type="string" column="state" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="startedTimestamp" type="integer" column="started_timestamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="templateId" type="integer" column="template_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
