<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ObjUserStat" table="obj_user_stat">
    <id name="objId" type="integer" column="obj_id"/>
    <id name="fulldate" type="integer" column="fulldate"/>
    <field name="yyyy" type="smallint" column="yyyy" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="mm" type="boolean" column="mm" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="dd" type="boolean" column="dd" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="counter" type="integer" column="counter" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
