<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LikeData" table="like_data">
    <indexes>
      <index name="i1_idx" columns="obj_id"/>
    </indexes>
    <id name="userId" type="integer" column="user_id"/>
    <id name="objId" type="integer" column="obj_id"/>
    <id name="objType" type="string" column="obj_type" length="40"/>
    <id name="subObjId" type="integer" column="sub_obj_id"/>
    <id name="subObjType" type="string" column="sub_obj_type" length="40"/>
    <id name="newsId" type="integer" column="news_id"/>
    <id name="likeType" type="integer" column="like_type"/>
    <field name="expTs" type="datetime" column="exp_ts" nullable="false"/>
  </entity>
</doctrine-mapping>
