<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TstDynQuestSetCfg" table="tst_dyn_quest_set_cfg">
    <id name="testFi" type="integer" column="test_fi">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sourceQplFi" type="integer" column="source_qpl_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="taxFilterEnabled" type="boolean" column="tax_filter_enabled" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="orderTax" type="integer" column="order_tax" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="sourceQplTitle" type="string" column="source_qpl_title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="answerFilterEnabled" type="boolean" column="answer_filter_enabled" nullable="true"/>
  </entity>
</doctrine-mapping>
