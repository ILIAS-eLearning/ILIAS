<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BookingUser" table="booking_user">
    <id name="entryId" type="integer" column="entry_id"/>
    <id name="userId" type="integer" column="user_id"/>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="bookingMessage" type="string" column="booking_message" length="1024" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="notificationSent" type="boolean" column="notification_sent" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
