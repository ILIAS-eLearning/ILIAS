<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BookingReservation" table="booking_reservation">
    <indexes>
      <index name="i1_idx" columns="user_id"/>
      <index name="i2_idx" columns="object_id"/>
    </indexes>
    <id name="bookingReservationId" type="integer" column="booking_reservation_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="objectId" type="integer" column="object_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="dateFrom" type="integer" column="date_from" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="dateTo" type="integer" column="date_to" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="status" type="smallint" column="status" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="groupId" type="integer" column="group_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="assignerId" type="integer" column="assigner_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
