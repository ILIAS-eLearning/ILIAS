<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplFbSpecific" table="qpl_fb_specific">
    <indexes>
      <index name="i1_idx" columns="question_fi"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="con_idx" columns="question_fi,question,answer"/>
    </unique-constraints>
    <id name="feedbackId" type="integer" column="feedback_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="questionFi" type="integer" column="question_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="answer" type="integer" column="answer" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="feedback" type="text" column="feedback" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="question" type="integer" column="question" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
