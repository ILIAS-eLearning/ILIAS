<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ObjLpStat" table="obj_lp_stat">
    <id name="objId" type="integer" column="obj_id"/>
    <id name="fulldate" type="integer" column="fulldate"/>
    <field name="type" type="string" column="type" length="4" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="yyyy" type="smallint" column="yyyy" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="mm" type="boolean" column="mm" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="dd" type="boolean" column="dd" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="memCnt" type="integer" column="mem_cnt" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="inProgress" type="integer" column="in_progress" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="completed" type="integer" column="completed" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="failed" type="integer" column="failed" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="notAttempted" type="integer" column="not_attempted" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
