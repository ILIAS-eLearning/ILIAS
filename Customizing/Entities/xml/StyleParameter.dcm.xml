<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="StyleParameter" table="style_parameter">
    <indexes>
      <index name="i1_idx" columns="style_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="styleId" type="integer" column="style_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tag" type="string" column="tag" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="class" type="string" column="class" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="parameter" type="string" column="parameter" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="value" type="string" column="value" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mqId" type="integer" column="mq_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="custom" type="boolean" column="custom" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
