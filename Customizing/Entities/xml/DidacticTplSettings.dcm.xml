<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DidacticTplSettings" table="didactic_tpl_settings">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="enabled" type="boolean" column="enabled" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="type" type="boolean" column="type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="info" type="string" column="info" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="autoGenerated" type="boolean" column="auto_generated" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="exclusiveTpl" type="boolean" column="exclusive_tpl" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
