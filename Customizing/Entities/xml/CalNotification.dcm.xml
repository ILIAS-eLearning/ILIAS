<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CalNotification" table="cal_notification">
    <indexes>
      <index name="i1_idx" columns="cal_id"/>
    </indexes>
    <id name="notificationId" type="integer" column="notification_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="calId" type="integer" column="cal_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="userType" type="boolean" column="user_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="email" type="string" column="email" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
