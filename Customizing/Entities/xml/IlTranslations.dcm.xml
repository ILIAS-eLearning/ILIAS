<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlTranslations" table="il_translations">
    <id name="id" type="integer" column="id"/>
    <id name="idType" type="string" column="id_type" length="50"/>
    <id name="langCode" type="string" column="lang_code" length="2"/>
    <field name="title" type="string" column="title" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="langDefault" type="boolean" column="lang_default" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
