<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlDclField" table="il_dcl_field">
    <indexes>
      <index name="i1_idx" columns="datatype_id"/>
      <index name="i2_idx" columns="table_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tableId" type="integer" column="table_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="datatypeId" type="integer" column="datatype_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="required" type="boolean" column="required" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isUnique" type="boolean" column="is_unique" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isLocked" type="boolean" column="is_locked" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
