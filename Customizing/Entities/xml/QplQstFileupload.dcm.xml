<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplQstFileupload" table="qpl_qst_fileupload">
    <id name="questionFi" type="integer" column="question_fi">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="allowedextensions" type="string" column="allowedextensions" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="maxsize" type="float" column="maxsize" precision="10" scale="0" nullable="true"/>
    <field name="complBySubmission" type="boolean" column="compl_by_submission" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
