<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Lti2UserResult" table="lti2_user_result">
    <indexes>
      <index name="i1_idx" columns="resource_link_pk"/>
    </indexes>
    <id name="userPk" type="integer" column="user_pk">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="resourceLinkPk" type="integer" column="resource_link_pk" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="ltiUserId" type="string" column="lti_user_id" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="ltiResultSourcedid" type="string" column="lti_result_sourcedid" length="1024" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="created" type="datetime" column="created" nullable="false">
      <options>
        <option name="default">1970-01-01 00:00:00</option>
      </options>
    </field>
    <field name="updated" type="datetime" column="updated" nullable="false">
      <options>
        <option name="default">1970-01-01 00:00:00</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
