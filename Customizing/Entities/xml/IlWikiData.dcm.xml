<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlWikiData" table="il_wiki_data">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="startpage" type="string" column="startpage" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="short" type="string" column="short" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isOnline" type="boolean" column="is_online" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="rating" type="boolean" column="rating" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="introduction" type="text" column="introduction" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="publicNotes" type="boolean" column="public_notes" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="impPages" type="boolean" column="imp_pages" nullable="true"/>
    <field name="pageToc" type="boolean" column="page_toc" nullable="true"/>
    <field name="ratingSide" type="boolean" column="rating_side" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="ratingNew" type="boolean" column="rating_new" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="ratingExt" type="boolean" column="rating_ext" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="ratingOverall" type="boolean" column="rating_overall" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="emptyPageTempl" type="boolean" column="empty_page_templ" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="linkMdValues" type="boolean" column="link_md_values" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
