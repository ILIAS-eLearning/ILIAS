<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IassInfoSettings" table="iass_info_settings">
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="contact" type="string" column="contact" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="responsibility" type="string" column="responsibility" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mails" type="string" column="mails" length="300" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="consultationHours" type="string" column="consultation_hours" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
