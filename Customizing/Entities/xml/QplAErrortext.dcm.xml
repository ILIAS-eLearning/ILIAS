<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplAErrortext" table="qpl_a_errortext">
    <indexes>
      <index name="i1_idx" columns="question_fi"/>
    </indexes>
    <id name="answerId" type="integer" column="answer_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="questionFi" type="integer" column="question_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="textWrong" type="string" column="text_wrong" length="150" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="textCorrect" type="string" column="text_correct" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="points" type="float" column="points" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="sequence" type="smallint" column="sequence" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
