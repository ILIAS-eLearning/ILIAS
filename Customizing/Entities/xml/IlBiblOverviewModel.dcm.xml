<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlBiblOverviewModel" table="il_bibl_overview_model">
    <id name="ovmId" type="integer" column="ovm_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="literatureType" type="string" column="literature_type" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pattern" type="string" column="pattern" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fileTypeId" type="integer" column="file_type_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
