<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SearchCommandQueue" table="search_command_queue">
    <id name="objId" type="integer" column="obj_id"/>
    <id name="objType" type="string" column="obj_type" length="4"/>
    <id name="subId" type="integer" column="sub_id"/>
    <field name="subType" type="string" column="sub_type" length="4" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="command" type="string" column="command" length="16" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="lastUpdate" type="datetime" column="last_update" nullable="true"/>
    <field name="finished" type="boolean" column="finished" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
