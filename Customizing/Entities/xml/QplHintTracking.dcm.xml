<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplHintTracking" table="qpl_hint_tracking">
    <id name="qhtrTrackId" type="integer" column="qhtr_track_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="qhtrActiveFi" type="integer" column="qhtr_active_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="qhtrPass" type="integer" column="qhtr_pass" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="qhtrQuestionFi" type="integer" column="qhtr_question_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="qhtrHintFi" type="integer" column="qhtr_hint_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
