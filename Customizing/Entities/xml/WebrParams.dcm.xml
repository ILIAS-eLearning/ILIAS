<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WebrParams" table="webr_params">
    <indexes>
      <index name="i1_idx" columns="link_id"/>
    </indexes>
    <id name="paramId" type="integer" column="param_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="webrId" type="integer" column="webr_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="linkId" type="integer" column="link_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="name" type="string" column="name" length="128" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="value" type="boolean" column="value" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
