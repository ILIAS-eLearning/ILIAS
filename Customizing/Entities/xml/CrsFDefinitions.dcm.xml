<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CrsFDefinitions" table="crs_f_definitions">
    <id name="fieldId" type="integer" column="field_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="objId" type="integer" column="obj_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fieldName" type="string" column="field_name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fieldType" type="boolean" column="field_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fieldValues" type="text" column="field_values" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fieldRequired" type="boolean" column="field_required" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fieldValuesOpt" type="string" column="field_values_opt" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
