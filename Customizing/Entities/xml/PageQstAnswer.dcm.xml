<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PageQstAnswer" table="page_qst_answer">
    <id name="qstId" type="integer" column="qst_id"/>
    <id name="userId" type="integer" column="user_id"/>
    <field name="try" type="boolean" column="try" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="passed" type="boolean" column="passed" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="points" type="float" column="points" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="unlocked" type="boolean" column="unlocked" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
