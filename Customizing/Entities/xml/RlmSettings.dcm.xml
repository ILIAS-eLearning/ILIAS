<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RlmSettings" table="rlm_settings">
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="mid" type="integer" column="mid" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="organization" type="string" column="organization" length="400" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="localInformation" type="string" column="local_information" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="remoteLink" type="string" column="remote_link" length="400" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="availabilityType" type="boolean" column="availability_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
