<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LocUserResults" table="loc_user_results">
    <id name="userId" type="integer" column="user_id"/>
    <id name="courseId" type="integer" column="course_id"/>
    <id name="objectiveId" type="integer" column="objective_id"/>
    <id name="type" type="boolean" column="type"/>
    <field name="status" type="boolean" column="status" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="resultPerc" type="boolean" column="result_perc" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="limitPerc" type="boolean" column="limit_perc" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tries" type="boolean" column="tries" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isFinal" type="boolean" column="is_final" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
