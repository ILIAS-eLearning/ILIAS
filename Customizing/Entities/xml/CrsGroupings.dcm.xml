<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CrsGroupings" table="crs_groupings">
    <indexes>
      <index name="i1_idx" columns="crs_id"/>
    </indexes>
    <id name="crsGrpId" type="integer" column="crs_grp_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="crsRefId" type="integer" column="crs_ref_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="crsId" type="integer" column="crs_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="uniqueField" type="string" column="unique_field" length="32" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
