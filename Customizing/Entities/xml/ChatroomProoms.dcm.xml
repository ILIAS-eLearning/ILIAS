<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ChatroomProoms" table="chatroom_prooms">
    <indexes>
      <index name="i1_idx" columns="parent_id"/>
      <index name="i2_idx" columns="owner"/>
    </indexes>
    <id name="proomId" type="integer" column="proom_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="parentId" type="integer" column="parent_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="200" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="owner" type="integer" column="owner" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="created" type="integer" column="created" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="closed" type="integer" column="closed" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isPublic" type="boolean" column="is_public" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
