<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CrsObjectives" table="crs_objectives">
    <indexes>
      <index name="i1_idx" columns="crs_id"/>
    </indexes>
    <id name="objectiveId" type="integer" column="objective_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="crsId" type="integer" column="crs_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="70" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="position" type="integer" column="position" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="created" type="integer" column="created" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="passes" type="smallint" column="passes" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
