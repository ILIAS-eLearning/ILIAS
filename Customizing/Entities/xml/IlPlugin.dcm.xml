<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlPlugin" table="il_plugin">
    <id name="componentType" type="string" column="component_type" length="10"/>
    <id name="componentName" type="string" column="component_name" length="90"/>
    <id name="slotId" type="string" column="slot_id" length="10"/>
    <id name="name" type="string" column="name" length="40"/>
    <field name="lastUpdateVersion" type="string" column="last_update_version" length="10" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="true"/>
    <field name="dbVersion" type="integer" column="db_version" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pluginId" type="string" column="plugin_id" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
