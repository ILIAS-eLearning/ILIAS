<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CalCategories" table="cal_categories">
    <indexes>
      <index name="i2_idx" columns="obj_id"/>
      <index name="i3_idx" columns="type"/>
    </indexes>
    <id name="catId" type="integer" column="cat_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="objId" type="integer" column="obj_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="128" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="color" type="string" column="color" length="8" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="type" type="boolean" column="type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="locType" type="boolean" column="loc_type" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="remoteUrl" type="string" column="remote_url" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="remoteUser" type="string" column="remote_user" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="remotePass" type="string" column="remote_pass" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="remoteSync" type="datetime" column="remote_sync" nullable="true"/>
  </entity>
</doctrine-mapping>
