<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UsrSessionStats" table="usr_session_stats">
    <indexes>
      <index name="i1_idx" columns="slot_end"/>
    </indexes>
    <id name="slotBegin" type="integer" column="slot_begin">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="slotEnd" type="integer" column="slot_end" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="activeMin" type="integer" column="active_min" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="activeMax" type="integer" column="active_max" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="activeAvg" type="integer" column="active_avg" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="activeEnd" type="integer" column="active_end" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="opened" type="integer" column="opened" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="closedManual" type="integer" column="closed_manual" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="closedExpire" type="integer" column="closed_expire" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="closedIdle" type="integer" column="closed_idle" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="closedIdleFirst" type="integer" column="closed_idle_first" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="closedLimit" type="integer" column="closed_limit" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="closedLogin" type="integer" column="closed_login" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="maxSessions" type="integer" column="max_sessions" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="closedMisc" type="integer" column="closed_misc" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
