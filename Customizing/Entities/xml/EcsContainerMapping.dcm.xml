<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EcsContainerMapping" table="ecs_container_mapping">
    <id name="mappingId" type="integer" column="mapping_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="containerId" type="integer" column="container_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fieldName" type="string" column="field_name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mappingType" type="boolean" column="mapping_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="mappingValue" type="string" column="mapping_value" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateRangeStart" type="integer" column="date_range_start" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="dateRangeEnd" type="integer" column="date_range_end" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
