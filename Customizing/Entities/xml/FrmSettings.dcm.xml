<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FrmSettings" table="frm_settings">
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="defaultView" type="integer" column="default_view" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="anonymized" type="boolean" column="anonymized" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="statisticsEnabled" type="boolean" column="statistics_enabled" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="postActivation" type="boolean" column="post_activation" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="adminForceNoti" type="boolean" column="admin_force_noti" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="userToggleNoti" type="boolean" column="user_toggle_noti" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="presetSubject" type="boolean" column="preset_subject" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="notificationType" type="string" column="notification_type" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="addReSubject" type="boolean" column="add_re_subject" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="markModPosts" type="boolean" column="mark_mod_posts" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="threadSorting" type="integer" column="thread_sorting" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="threadRating" type="boolean" column="thread_rating" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fileUploadAllowed" type="boolean" column="file_upload_allowed" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
