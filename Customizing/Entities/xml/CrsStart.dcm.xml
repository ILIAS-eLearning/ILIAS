<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CrsStart" table="crs_start">
    <indexes>
      <index name="i1_idx" columns="crs_id"/>
    </indexes>
    <id name="crsStartId" type="integer" column="crs_start_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="crsId" type="integer" column="crs_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="itemRefId" type="integer" column="item_ref_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pos" type="integer" column="pos" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
