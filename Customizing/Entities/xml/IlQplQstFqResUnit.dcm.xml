<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlQplQstFqResUnit" table="il_qpl_qst_fq_res_unit">
    <indexes>
      <index name="i1_idx" columns="question_fi,unit_fi"/>
    </indexes>
    <id name="resultUnitId" type="integer" column="result_unit_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="result" type="string" column="result" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="questionFi" type="integer" column="question_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="unitFi" type="integer" column="unit_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
