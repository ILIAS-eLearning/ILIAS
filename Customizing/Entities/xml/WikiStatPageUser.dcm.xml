<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WikiStatPageUser" table="wiki_stat_page_user">
    <id name="wikiId" type="integer" column="wiki_id"/>
    <id name="pageId" type="integer" column="page_id"/>
    <id name="userId" type="integer" column="user_id"/>
    <id name="ts" type="datetime" column="ts"/>
    <field name="changes" type="integer" column="changes" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="readEvents" type="integer" column="read_events" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tsDay" type="string" column="ts_day" length="10" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="tsHour" type="boolean" column="ts_hour" nullable="true"/>
  </entity>
</doctrine-mapping>
