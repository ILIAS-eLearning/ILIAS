<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PageStyleUsage" table="page_style_usage">
    <indexes>
      <index name="i1_idx" columns="page_id,page_type,page_lang,page_nr"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="pageId" type="integer" column="page_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pageType" type="string" column="page_type" length="10" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default"/>
      </options>
    </field>
    <field name="pageNr" type="integer" column="page_nr" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="template" type="boolean" column="template" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="stype" type="string" column="stype" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sname" type="string" column="sname" length="30" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="pageLang" type="string" column="page_lang" length="2" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">-</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
