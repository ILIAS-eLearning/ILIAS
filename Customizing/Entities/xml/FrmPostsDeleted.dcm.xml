<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FrmPostsDeleted" table="frm_posts_deleted">
    <id name="deletedId" type="integer" column="deleted_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="false">
      <options>
        <option name="default">1970-01-01 00:00:00</option>
      </options>
    </field>
    <field name="deletedBy" type="string" column="deleted_by" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="forumTitle" type="string" column="forum_title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="threadTitle" type="string" column="thread_title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="postTitle" type="string" column="post_title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="postMessage" type="text" column="post_message" length="0" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="postDate" type="datetime" column="post_date" nullable="false">
      <options>
        <option name="default">1970-01-01 00:00:00</option>
      </options>
    </field>
    <field name="objId" type="integer" column="obj_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="refId" type="integer" column="ref_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="threadId" type="integer" column="thread_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="forumId" type="integer" column="forum_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="posDisplayUserId" type="integer" column="pos_display_user_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="posUsrAlias" type="string" column="pos_usr_alias" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isThreadDeleted" type="boolean" column="is_thread_deleted" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
