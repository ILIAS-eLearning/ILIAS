<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CalAuthToken" table="cal_auth_token">
    <indexes>
      <index name="i1_idx" columns="hash"/>
    </indexes>
    <id name="userId" type="integer" column="user_id"/>
    <id name="hash" type="string" column="hash" length="32"/>
    <field name="selection" type="integer" column="selection" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="calendar" type="integer" column="calendar" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="ical" type="text" column="ical" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cTime" type="integer" column="c_time" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
