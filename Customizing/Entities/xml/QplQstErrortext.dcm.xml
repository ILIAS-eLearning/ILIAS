<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplQstErrortext" table="qpl_qst_errortext">
    <id name="questionFi" type="integer" column="question_fi">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="errortext" type="string" column="errortext" length="4000" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="textsize" type="float" column="textsize" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">100</option>
      </options>
    </field>
    <field name="pointsWrong" type="float" column="points_wrong" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">-1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
