<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UsrSession" table="usr_session">
    <indexes>
      <index name="i1_idx" columns="expires"/>
      <index name="i2_idx" columns="user_id"/>
    </indexes>
    <id name="sessionId" type="string" column="session_id" length="250">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="expires" type="integer" column="expires" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="data" type="text" column="data" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ctime" type="integer" column="ctime" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lastRemindTs" type="integer" column="last_remind_ts" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="type" type="integer" column="type" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createtime" type="integer" column="createtime" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="remoteAddr" type="string" column="remote_addr" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="context" type="string" column="context" length="80" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
