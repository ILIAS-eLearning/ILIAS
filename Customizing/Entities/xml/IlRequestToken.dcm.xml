<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlRequestToken" table="il_request_token">
    <indexes>
      <index name="i1_idx" columns="user_id,session_id"/>
      <index name="i2_idx" columns="user_id,stamp"/>
    </indexes>
    <id name="token" type="string" column="token" length="64">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="stamp" type="datetime" column="stamp" nullable="true"/>
    <field name="sessionId" type="string" column="session_id" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
