<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CalChGroup" table="cal_ch_group">
    <id name="grpId" type="integer" column="grp_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="usrId" type="integer" column="usr_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="multipleAssignments" type="boolean" column="multiple_assignments" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
