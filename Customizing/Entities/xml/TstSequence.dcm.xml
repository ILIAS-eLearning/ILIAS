<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TstSequence" table="tst_sequence">
    <id name="activeFi" type="integer" column="active_fi"/>
    <id name="pass" type="integer" column="pass"/>
    <field name="sequence" type="text" column="sequence" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="postponed" type="string" column="postponed" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hidden" type="string" column="hidden" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="ansOptConfirmed" type="boolean" column="ans_opt_confirmed" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
