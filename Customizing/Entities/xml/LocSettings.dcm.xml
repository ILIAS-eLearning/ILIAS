<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LocSettings" table="loc_settings">
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="boolean" column="type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="itest" type="integer" column="itest" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="qtest" type="integer" column="qtest" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="qtVisAll" type="boolean" column="qt_vis_all" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="qtVisObj" type="boolean" column="qt_vis_obj" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="resetResults" type="boolean" column="reset_results" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="itType" type="boolean" column="it_type" nullable="true">
      <options>
        <option name="default">5</option>
      </options>
    </field>
    <field name="qtType" type="boolean" column="qt_type" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="itStart" type="boolean" column="it_start" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="qtStart" type="boolean" column="qt_start" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="passedObjMode" type="boolean" column="passed_obj_mode" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
