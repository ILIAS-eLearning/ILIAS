<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SvyQuestion" table="svy_question">
    <indexes>
      <index name="i1_idx" columns="obj_fi"/>
      <index name="i2_idx" columns="owner_fi"/>
    </indexes>
    <id name="questionId" type="integer" column="question_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="questiontypeFi" type="integer" column="questiontype_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="objFi" type="integer" column="obj_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="ownerFi" type="integer" column="owner_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="author" type="string" column="author" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="obligatory" type="string" column="obligatory" length="1" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="complete" type="string" column="complete" length="1" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="created" type="string" column="created" length="14" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="originalId" type="integer" column="original_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="questiontext" type="text" column="questiontext" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="label" type="string" column="label" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
