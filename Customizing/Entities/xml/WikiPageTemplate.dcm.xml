<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WikiPageTemplate" table="wiki_page_template">
    <id name="wikiId" type="integer" column="wiki_id"/>
    <id name="wpageId" type="integer" column="wpage_id"/>
    <field name="newPages" type="boolean" column="new_pages" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="addToPage" type="boolean" column="add_to_page" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
