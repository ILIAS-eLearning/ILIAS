<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TosVersions" table="tos_versions">
    <indexes>
      <index name="i1_idx" columns="hash,doc_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="text" type="text" column="text" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hash" type="string" column="hash" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ts" type="integer" column="ts" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="docId" type="integer" column="doc_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="text" column="title" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
