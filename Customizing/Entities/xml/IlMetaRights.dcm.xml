<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlMetaRights" table="il_meta_rights">
    <indexes>
      <index name="i1_idx" columns="rbac_id,obj_id"/>
    </indexes>
    <id name="metaRightsId" type="integer" column="meta_rights_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rbacId" type="integer" column="rbac_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="objId" type="integer" column="obj_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="objType" type="string" column="obj_type" length="6" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="costs" type="string" column="costs" length="3" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="cprAndOr" type="string" column="cpr_and_or" length="3" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="description" type="string" column="description" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descriptionLanguage" type="string" column="description_language" length="2" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
