<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UtLpMarks" table="ut_lp_marks">
    <id name="objId" type="integer" column="obj_id"/>
    <id name="usrId" type="integer" column="usr_id"/>
    <field name="completed" type="integer" column="completed" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="mark" type="string" column="mark" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="uComment" type="string" column="u_comment" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="statusChanged" type="datetime" column="status_changed" nullable="true"/>
    <field name="statusDirty" type="boolean" column="status_dirty" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="percentage" type="boolean" column="percentage" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
