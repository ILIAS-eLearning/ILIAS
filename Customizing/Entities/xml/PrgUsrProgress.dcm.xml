<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PrgUsrProgress" table="prg_usr_progress">
    <unique-constraints>
      <unique-constraint name="con_idx" columns="assignment_id,prg_id,usr_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="assignmentId" type="integer" column="assignment_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="prgId" type="integer" column="prg_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="usrId" type="integer" column="usr_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="points" type="integer" column="points" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pointsCur" type="integer" column="points_cur" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="completionBy" type="integer" column="completion_by" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lastChange" type="datetime" column="last_change" nullable="false">
      <options>
        <option name="default">1970-01-01 00:00:00</option>
      </options>
    </field>
    <field name="lastChangeBy" type="integer" column="last_change_by" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="deadline" type="string" column="deadline" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
