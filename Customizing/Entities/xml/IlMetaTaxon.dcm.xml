<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlMetaTaxon" table="il_meta_taxon">
    <indexes>
      <index name="i1_idx" columns="rbac_id,obj_id"/>
    </indexes>
    <id name="metaTaxonId" type="integer" column="meta_taxon_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rbacId" type="integer" column="rbac_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="objId" type="integer" column="obj_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="objType" type="string" column="obj_type" length="6" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="parentType" type="string" column="parent_type" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="parentId" type="integer" column="parent_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="taxon" type="string" column="taxon" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="taxonLanguage" type="string" column="taxon_language" length="2" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="taxonId" type="string" column="taxon_id" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
