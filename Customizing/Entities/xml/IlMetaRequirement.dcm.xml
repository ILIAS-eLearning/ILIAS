      <generator strategy="IDENTITY"/>
    </id>
    <field name="rbacId" type="integer" column="rbac_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="objId" type="integer" column="obj_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="objType" type="string" column="obj_type" length="6" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="parentType" type="string" column="parent_type" length="16" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="parentId" type="integer" column="parent_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="operatingSystemName" type="string" column="operating_system_name" length="16" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="osMinVersion" type="string" column="os_min_version" length="255" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="osMaxVersion" type="string" column="os_max_version" length="255" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="browserName" type="string" column="browser_name" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="browserMinimumVersion" type="string" column="browser_minimum_version" length="255" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="browserMaximumVersion" type="string" column="browser_maximum_version" length="255" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="orCompositeId" type="integer" column="or_composite_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
