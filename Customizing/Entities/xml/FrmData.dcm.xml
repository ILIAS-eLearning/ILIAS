<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FrmData" table="frm_data">
    <indexes>
      <index name="i1_idx" columns="top_frm_fk"/>
    </indexes>
    <id name="topPk" type="bigint" column="top_pk">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="topFrmFk" type="bigint" column="top_frm_fk" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="topName" type="string" column="top_name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="topDescription" type="string" column="top_description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="topNumPosts" type="integer" column="top_num_posts" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="topNumThreads" type="integer" column="top_num_threads" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="topLastPost" type="string" column="top_last_post" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="topMods" type="integer" column="top_mods" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="topDate" type="datetime" column="top_date" nullable="true"/>
    <field name="visits" type="integer" column="visits" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="topUpdate" type="datetime" column="top_update" nullable="true"/>
    <field name="updateUser" type="integer" column="update_user" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="topUsrId" type="bigint" column="top_usr_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
