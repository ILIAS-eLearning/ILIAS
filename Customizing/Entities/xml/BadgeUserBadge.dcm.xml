<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BadgeUserBadge" table="badge_user_badge">
    <id name="badgeId" type="integer" column="badge_id"/>
    <id name="userId" type="integer" column="user_id"/>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="awardedBy" type="integer" column="awarded_by" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="pos" type="smallint" column="pos" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
