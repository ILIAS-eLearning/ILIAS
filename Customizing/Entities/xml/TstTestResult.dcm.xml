<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TstTestResult" table="tst_test_result">
    <indexes>
      <index name="i1_idx" columns="active_fi"/>
      <index name="i3_idx" columns="pass"/>
      <index name="i2_idx" columns="question_fi"/>
    </indexes>
    <id name="testResultId" type="integer" column="test_result_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="activeFi" type="integer" column="active_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="questionFi" type="integer" column="question_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="points" type="float" column="points" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pass" type="integer" column="pass" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="manual" type="boolean" column="manual" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="hintCount" type="integer" column="hint_count" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="hintPoints" type="float" column="hint_points" precision="10" scale="0" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="answered" type="boolean" column="answered" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="step" type="integer" column="step" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
