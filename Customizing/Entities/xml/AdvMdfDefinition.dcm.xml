<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdvMdfDefinition" table="adv_mdf_definition">
    <id name="fieldId" type="integer" column="field_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="recordId" type="integer" column="record_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="importId" type="string" column="import_id" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="position" type="integer" column="position" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fieldType" type="boolean" column="field_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fieldValues" type="string" column="field_values" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="2000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="searchable" type="boolean" column="searchable" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="required" type="boolean" column="required" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
