<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ShibRoleAssignment" table="shib_role_assignment">
    <id name="ruleId" type="integer" column="rule_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="roleId" type="integer" column="role_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="name" type="string" column="name" length="255" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="value" type="string" column="value" length="255" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="plugin" type="boolean" column="plugin" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pluginId" type="integer" column="plugin_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="addOnUpdate" type="boolean" column="add_on_update" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="removeOnUpdate" type="boolean" column="remove_on_update" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
