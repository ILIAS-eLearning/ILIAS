<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplQstEssay" table="qpl_qst_essay">
    <id name="questionFi" type="integer" column="question_fi">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="maxnumofchars" type="integer" column="maxnumofchars" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="keywords" type="string" column="keywords" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="textgapRating" type="string" column="textgap_rating" length="2" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="matchcondition" type="smallint" column="matchcondition" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="keywordRelation" type="string" column="keyword_relation" length="3" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">any</option>
      </options>
    </field>
    <field name="wordCntEnabled" type="boolean" column="word_cnt_enabled" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
