<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlBtBucket" table="il_bt_bucket">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="bigint" column="user_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="rootTaskId" type="bigint" column="root_task_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="currentTaskId" type="bigint" column="current_task_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="state" type="smallint" column="state" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="totalNumberOfTasks" type="integer" column="total_number_of_tasks" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="percentage" type="smallint" column="percentage" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastHeartbeat" type="integer" column="last_heartbeat" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
