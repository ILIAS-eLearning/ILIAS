<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlBtValue" table="il_bt_value">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="hasParentTask" type="boolean" column="has_parent_task" nullable="true"/>
    <field name="parentTaskId" type="bigint" column="parent_task_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hash" type="string" column="hash" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="classPath" type="string" column="class_path" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="className" type="string" column="class_name" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="serialized" type="text" column="serialized" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bucketId" type="bigint" column="bucket_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
