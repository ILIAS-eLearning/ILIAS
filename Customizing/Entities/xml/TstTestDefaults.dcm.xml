<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TstTestDefaults" table="tst_test_defaults">
    <indexes>
      <index name="i1_idx" columns="user_fi"/>
    </indexes>
    <id name="testDefaultsId" type="integer" column="test_defaults_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userFi" type="integer" column="user_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="name" type="string" column="name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="marks" type="text" column="marks" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="defaults" type="text" column="defaults" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
