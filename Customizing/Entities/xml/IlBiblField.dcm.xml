<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlBiblField" table="il_bibl_field">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="identifier" type="string" column="identifier" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dataType" type="boolean" column="data_type" nullable="false"/>
    <field name="position" type="integer" column="position" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="isStandardField" type="boolean" column="is_standard_field" nullable="false"/>
    <field name="objectId" type="integer" column="object_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
