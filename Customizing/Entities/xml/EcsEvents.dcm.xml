<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EcsEvents" table="ecs_events">
    <id name="eventId" type="integer" column="event_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="type" length="32" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="id" type="integer" column="id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="op" type="string" column="op" length="32" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="serverId" type="integer" column="server_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
