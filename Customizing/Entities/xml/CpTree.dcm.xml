<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CpTree" table="cp_tree">
    <indexes>
      <index name="i2_idx" columns="obj_id"/>
      <index name="i1_idx" columns="child"/>
      <index name="i3_idx" columns="parent"/>
    </indexes>
    <id name="child" type="integer" column="child"/>
    <id name="objId" type="integer" column="obj_id"/>
    <field name="depth" type="integer" column="depth" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lft" type="integer" column="lft" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="parent" type="integer" column="parent" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="rgt" type="integer" column="rgt" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
