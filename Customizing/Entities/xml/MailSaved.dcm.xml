<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MailSaved" table="mail_saved">
    <id name="userId" type="integer" column="user_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="mType" type="string" column="m_type" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mEmail" type="boolean" column="m_email" nullable="true"/>
    <field name="mSubject" type="string" column="m_subject" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usePlaceholders" type="boolean" column="use_placeholders" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="mMessage" type="text" column="m_message" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rcpTo" type="text" column="rcp_to" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rcpCc" type="text" column="rcp_cc" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rcpBcc" type="text" column="rcp_bcc" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="attachments" type="text" column="attachments" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tplCtxId" type="string" column="tpl_ctx_id" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tplCtxParams" type="blob" column="tpl_ctx_params" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
