<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplSolSug" table="qpl_sol_sug">
    <indexes>
      <index name="i1_idx" columns="question_fi"/>
    </indexes>
    <id name="suggestedSolutionId" type="integer" column="suggested_solution_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="questionFi" type="integer" column="question_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="internalLink" type="string" column="internal_link" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="importId" type="string" column="import_id" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="subquestionIndex" type="integer" column="subquestion_index" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="type" type="string" column="type" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="value" type="text" column="value" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
