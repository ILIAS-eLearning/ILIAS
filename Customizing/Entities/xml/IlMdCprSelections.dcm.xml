<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlMdCprSelections" table="il_md_cpr_selections">
    <id name="entryId" type="integer" column="entry_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="copyright" type="text" column="copyright" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="language" type="string" column="language" length="2" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="costs" type="boolean" column="costs" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="cprRestrictions" type="boolean" column="cpr_restrictions" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="isDefault" type="boolean" column="is_default" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="outdated" type="boolean" column="outdated" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="position" type="boolean" column="position" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
