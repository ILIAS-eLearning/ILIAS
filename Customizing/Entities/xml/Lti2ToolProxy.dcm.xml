<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Lti2ToolProxy" table="lti2_tool_proxy">
    <indexes>
      <index name="i1_idx" columns="consumer_pk"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="u1_idx" columns="tool_proxy_id"/>
    </unique-constraints>
    <id name="toolProxyPk" type="integer" column="tool_proxy_pk">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="toolProxyId" type="string" column="tool_proxy_id" length="32" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="consumerPk" type="integer" column="consumer_pk" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="toolProxy" type="blob" column="tool_proxy" length="0" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="created" type="datetime" column="created" nullable="false">
      <options>
        <option name="default">1970-01-01 00:00:00</option>
      </options>
    </field>
    <field name="updated" type="datetime" column="updated" nullable="false">
      <options>
        <option name="default">1970-01-01 00:00:00</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
