<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DidacticTplFp" table="didactic_tpl_fp">
    <id name="patternId" type="integer" column="pattern_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="patternType" type="boolean" column="pattern_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="patternSubType" type="boolean" column="pattern_sub_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pattern" type="string" column="pattern" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="parentId" type="integer" column="parent_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="parentType" type="string" column="parent_type" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
