<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplQstMc" table="qpl_qst_mc">
    <id name="questionFi" type="integer" column="question_fi">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="shuffle" type="string" column="shuffle" length="1" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="allowImages" type="string" column="allow_images" length="1" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="resizeImages" type="string" column="resize_images" length="1" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="thumbSize" type="smallint" column="thumb_size" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="feedbackSetting" type="boolean" column="feedback_setting" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="selectionLimit" type="integer" column="selection_limit" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
