<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Event" table="event">
    <indexes>
      <index name="i1_idx" columns="obj_id"/>
    </indexes>
    <id name="eventId" type="integer" column="event_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="objId" type="integer" column="obj_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="70" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="location" type="string" column="location" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tutorName" type="string" column="tutor_name" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tutorEmail" type="string" column="tutor_email" length="127" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tutorPhone" type="string" column="tutor_phone" length="127" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="details" type="string" column="details" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="registration" type="boolean" column="registration" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="participation" type="boolean" column="participation" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="regType" type="smallint" column="reg_type" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="regLimitUsers" type="integer" column="reg_limit_users" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="regWaitingList" type="boolean" column="reg_waiting_list" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="regLimited" type="boolean" column="reg_limited" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="regMinUsers" type="smallint" column="reg_min_users" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="regAutoWait" type="boolean" column="reg_auto_wait" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="showMembers" type="boolean" column="show_members" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="mailMembers" type="boolean" column="mail_members" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
