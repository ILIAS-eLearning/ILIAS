<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ExportOptions" table="export_options">
    <id name="exportId" type="smallint" column="export_id"/>
    <id name="keyword" type="smallint" column="keyword"/>
    <id name="refId" type="integer" column="ref_id"/>
    <field name="objId" type="integer" column="obj_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="value" type="string" column="value" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pos" type="integer" column="pos" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
