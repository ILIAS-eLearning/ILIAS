<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SvySkillThreshold" table="svy_skill_threshold">
    <id name="surveyId" type="integer" column="survey_id"/>
    <id name="baseSkillId" type="integer" column="base_skill_id"/>
    <id name="trefId" type="integer" column="tref_id"/>
    <id name="levelId" type="integer" column="level_id"/>
    <field name="threshold" type="integer" column="threshold" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
