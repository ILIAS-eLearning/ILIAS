<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PersonalPcClipboard" table="personal_pc_clipboard">
    <indexes>
      <index name="i1_idx" columns="user_id"/>
    </indexes>
    <id name="userId" type="integer" column="user_id"/>
    <id name="insertTime" type="datetime" column="insert_time"/>
    <id name="orderNr" type="integer" column="order_nr"/>
    <field name="content" type="text" column="content" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
