<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OscMessages" table="osc_messages">
    <indexes>
      <index name="i1_idx" columns="user_id"/>
      <index name="i3_idx" columns="conversation_id,user_id,timestamp"/>
      <index name="i2_idx" columns="conversation_id"/>
    </indexes>
    <id name="id" type="string" column="id" length="255">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="conversationId" type="string" column="conversation_id" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="message" type="text" column="message" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="timestamp" type="bigint" column="timestamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
