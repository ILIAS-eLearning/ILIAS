<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CopgPcDef" table="copg_pc_def">
    <id name="pcType" type="string" column="pc_type" length="20">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="40" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="directory" type="string" column="directory" length="40" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="intLinks" type="boolean" column="int_links" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="styleClasses" type="boolean" column="style_classes" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="xsl" type="boolean" column="xsl" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="component" type="string" column="component" length="40" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="defEnabled" type="boolean" column="def_enabled" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
