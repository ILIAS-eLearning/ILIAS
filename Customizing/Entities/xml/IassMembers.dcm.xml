<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IassMembers" table="iass_members">
    <id name="objId" type="integer" column="obj_id"/>
    <id name="usrId" type="integer" column="usr_id"/>
    <field name="examinerId" type="integer" column="examiner_id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="record" type="string" column="record" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="internalNote" type="string" column="internal_note" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="notify" type="boolean" column="notify" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="notificationTs" type="integer" column="notification_ts" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">-1</option>
      </options>
    </field>
    <field name="learningProgress" type="boolean" column="learning_progress" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="finalized" type="boolean" column="finalized" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="place" type="string" column="place" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="eventTime" type="bigint" column="event_time" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fileName" type="string" column="file_name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userViewFile" type="boolean" column="user_view_file" nullable="true"/>
    <field name="changerId" type="integer" column="changer_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="changeTime" type="string" column="change_time" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
