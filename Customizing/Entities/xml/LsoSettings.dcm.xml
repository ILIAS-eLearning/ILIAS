<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LsoSettings" table="lso_settings">
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="abstract" type="text" column="abstract" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="extro" type="text" column="extro" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="abstractImage" type="string" column="abstract_image" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="extroImage" type="string" column="extro_image" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gallery" type="boolean" column="gallery" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
