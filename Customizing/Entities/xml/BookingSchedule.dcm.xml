<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BookingSchedule" table="booking_schedule">
    <id name="bookingScheduleId" type="integer" column="booking_schedule_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="poolId" type="integer" column="pool_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="deadline" type="integer" column="deadline" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="rentMin" type="integer" column="rent_min" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="rentMax" type="integer" column="rent_max" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="raster" type="integer" column="raster" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="autoBreak" type="integer" column="auto_break" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="avFrom" type="integer" column="av_from" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="avTo" type="integer" column="av_to" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
