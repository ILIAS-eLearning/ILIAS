<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlCldData" table="il_cld_data">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="isOnline" type="boolean" column="is_online" nullable="true"/>
    <field name="service" type="string" column="service" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rootFolder" type="string" column="root_folder" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rootId" type="string" column="root_id" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ownerId" type="bigint" column="owner_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="authComplete" type="boolean" column="auth_complete" nullable="true"/>
  </entity>
</doctrine-mapping>
