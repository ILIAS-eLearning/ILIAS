<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SvyQstText" table="svy_qst_text">
    <id name="questionFi" type="integer" column="question_fi">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="maxchars" type="integer" column="maxchars" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="width" type="integer" column="width" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">50</option>
      </options>
    </field>
    <field name="height" type="integer" column="height" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">5</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
