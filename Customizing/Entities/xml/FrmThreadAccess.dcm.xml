<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FrmThreadAccess" table="frm_thread_access">
    <indexes>
      <index name="i1_idx" columns="access_last"/>
    </indexes>
    <id name="usrId" type="integer" column="usr_id"/>
    <id name="objId" type="integer" column="obj_id"/>
    <id name="threadId" type="integer" column="thread_id"/>
    <field name="accessOld" type="integer" column="access_old" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="accessLast" type="integer" column="access_last" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="accessOldTs" type="datetime" column="access_old_ts" nullable="true"/>
  </entity>
</doctrine-mapping>
