<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CpRule" table="cp_rule">
    <id name="cpNodeId" type="integer" column="cp_node_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="action" type="string" column="action" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="childactivityset" type="string" column="childactivityset" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="conditioncombination" type="string" column="conditioncombination" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="minimumcount" type="integer" column="minimumcount" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="minimumpercent" type="string" column="minimumpercent" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cType" type="string" column="c_type" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
