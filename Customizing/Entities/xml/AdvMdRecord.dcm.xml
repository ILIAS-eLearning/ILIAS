<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdvMdRecord" table="adv_md_record">
    <id name="recordId" type="integer" column="record_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="importId" type="string" column="import_id" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="parentObj" type="integer" column="parent_obj" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="gpos" type="integer" column="gpos" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
