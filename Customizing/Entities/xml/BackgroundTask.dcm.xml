<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BackgroundTask" table="background_task">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="handler" type="string" column="handler" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="steps" type="integer" column="steps" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="cstep" type="integer" column="cstep" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="startDate" type="datetime" column="start_date" nullable="true"/>
    <field name="status" type="string" column="status" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="params" type="string" column="params" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
