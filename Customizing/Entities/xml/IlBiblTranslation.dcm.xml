<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlBiblTranslation" table="il_bibl_translation">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fieldId" type="bigint" column="field_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="languageKey" type="string" column="language_key" length="2" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="translation" type="string" column="translation" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
