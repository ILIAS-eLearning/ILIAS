<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlMetaGeneral" table="il_meta_general">
    <indexes>
      <index name="i1_idx" columns="rbac_id,obj_id"/>
    </indexes>
    <id name="metaGeneralId" type="integer" column="meta_general_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rbacId" type="integer" column="rbac_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="objId" type="integer" column="obj_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="objType" type="string" column="obj_type" length="6" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="generalStructure" type="string" column="general_structure" length="16" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="titleLanguage" type="string" column="title_language" length="2" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="coverage" type="string" column="coverage" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="coverageLanguage" type="string" column="coverage_language" length="2" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
