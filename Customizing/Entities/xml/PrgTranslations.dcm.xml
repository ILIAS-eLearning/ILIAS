<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PrgTranslations" table="prg_translations">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="prgTypeId" type="integer" column="prg_type_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lang" type="string" column="lang" length="4" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="member" type="string" column="member" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="value" type="string" column="value" length="3500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
