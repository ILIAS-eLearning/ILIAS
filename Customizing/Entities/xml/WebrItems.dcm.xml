<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WebrItems" table="webr_items">
    <indexes>
      <index name="i1_idx" columns="link_id,webr_id"/>
    </indexes>
    <id name="linkId" type="integer" column="link_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="webrId" type="integer" column="webr_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="127" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="target" type="string" column="target" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="true"/>
    <field name="disableCheck" type="boolean" column="disable_check" nullable="true"/>
    <field name="createDate" type="integer" column="create_date" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lastUpdate" type="integer" column="last_update" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lastCheck" type="integer" column="last_check" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="valid" type="boolean" column="valid" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="internal" type="boolean" column="internal" nullable="true"/>
  </entity>
</doctrine-mapping>
