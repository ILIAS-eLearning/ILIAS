<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CpMapinfo" table="cp_mapinfo">
    <indexes>
      <index name="i1_idx" columns="targetobjectiveid"/>
    </indexes>
    <id name="cpNodeId" type="integer" column="cp_node_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="readnormalmeasure" type="boolean" column="readnormalmeasure" nullable="true"/>
    <field name="readsatisfiedstatus" type="boolean" column="readsatisfiedstatus" nullable="true"/>
    <field name="targetobjectiveid" type="string" column="targetobjectiveid" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="writenormalmeasure" type="boolean" column="writenormalmeasure" nullable="true"/>
    <field name="writesatisfiedstatus" type="boolean" column="writesatisfiedstatus" nullable="true"/>
  </entity>
</doctrine-mapping>
