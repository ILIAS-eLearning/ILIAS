<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="StyleChar" table="style_char">
    <indexes>
      <index name="i1_idx" columns="style_id"/>
    </indexes>
    <id name="styleId" type="integer" column="style_id"/>
    <id name="type" type="string" column="type" length="30"/>
    <id name="characteristic" type="string" column="characteristic" length="30"/>
    <field name="hide" type="boolean" column="hide" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
