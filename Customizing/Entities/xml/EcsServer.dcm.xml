<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EcsServer" table="ecs_server">
    <id name="serverId" type="integer" column="server_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="active" type="boolean" column="active" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="protocol" type="boolean" column="protocol" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="server" type="string" column="server" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="port" type="smallint" column="port" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="authType" type="boolean" column="auth_type" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="clientCertPath" type="string" column="client_cert_path" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="caCertPath" type="string" column="ca_cert_path" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="keyPath" type="string" column="key_path" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="keyPassword" type="string" column="key_password" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="certSerial" type="string" column="cert_serial" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pollingTime" type="integer" column="polling_time" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="importId" type="integer" column="import_id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="globalRole" type="integer" column="global_role" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="econtentRcp" type="string" column="econtent_rcp" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userRcp" type="string" column="user_rcp" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="approvalRcp" type="string" column="approval_rcp" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="duration" type="integer" column="duration" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="authUser" type="string" column="auth_user" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="authPass" type="string" column="auth_pass" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
