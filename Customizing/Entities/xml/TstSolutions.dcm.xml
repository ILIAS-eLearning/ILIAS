<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TstSolutions" table="tst_solutions">
    <indexes>
      <index name="i1_idx" columns="question_fi"/>
      <index name="i2_idx" columns="active_fi"/>
    </indexes>
    <id name="solutionId" type="integer" column="solution_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="activeFi" type="integer" column="active_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="questionFi" type="integer" column="question_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="points" type="float" column="points" precision="10" scale="0" nullable="true"/>
    <field name="pass" type="integer" column="pass" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="value1" type="text" column="value1" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="value2" type="text" column="value2" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="step" type="integer" column="step" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="authorized" type="boolean" column="authorized" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
