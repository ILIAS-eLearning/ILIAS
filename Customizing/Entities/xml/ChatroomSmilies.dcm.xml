<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ChatroomSmilies" table="chatroom_smilies">
    <id name="smileyId" type="integer" column="smiley_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="smileyKeywords" type="string" column="smiley_keywords" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="smileyPath" type="string" column="smiley_path" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
