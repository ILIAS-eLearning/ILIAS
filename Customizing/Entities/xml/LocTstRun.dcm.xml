<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LocTstRun" table="loc_tst_run">
    <id name="containerId" type="integer" column="container_id"/>
    <id name="userId" type="integer" column="user_id"/>
    <id name="testId" type="integer" column="test_id"/>
    <id name="objectiveId" type="integer" column="objective_id"/>
    <field name="maxPoints" type="integer" column="max_points" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="questions" type="string" column="questions" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
