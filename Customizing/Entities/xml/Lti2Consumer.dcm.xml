<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Lti2Consumer" table="lti2_consumer">
    <id name="consumerPk" type="integer" column="consumer_pk">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="consumerKey256" type="string" column="consumer_key256" length="256" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="consumerKey" type="blob" column="consumer_key" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="secret" type="string" column="secret" length="1024" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="ltiVersion" type="string" column="lti_version" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="consumerName" type="string" column="consumer_name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="consumerVersion" type="string" column="consumer_version" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="consumerGuid" type="string" column="consumer_guid" length="1024" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="profile" type="blob" column="profile" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="toolProxy" type="blob" column="tool_proxy" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="settings" type="blob" column="settings" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="protected" type="boolean" column="protected" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="enabled" type="boolean" column="enabled" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="enableFrom" type="datetime" column="enable_from" nullable="true"/>
    <field name="enableUntil" type="datetime" column="enable_until" nullable="true"/>
    <field name="lastAccess" type="datetime" column="last_access" nullable="true"/>
    <field name="created" type="datetime" column="created" nullable="false">
      <options>
        <option name="default">1970-01-01 00:00:00</option>
      </options>
    </field>
    <field name="updated" type="datetime" column="updated" nullable="false">
      <options>
        <option name="default">1970-01-01 00:00:00</option>
      </options>
    </field>
    <field name="extConsumerId" type="integer" column="ext_consumer_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="refId" type="integer" column="ref_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
