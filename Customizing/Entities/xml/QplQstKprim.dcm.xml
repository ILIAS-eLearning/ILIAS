<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplQstKprim" table="qpl_qst_kprim">
    <id name="questionFi" type="integer" column="question_fi">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="shuffleAnswers" type="boolean" column="shuffle_answers" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="answerType" type="string" column="answer_type" length="16" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">singleLine</option>
      </options>
    </field>
    <field name="thumbSize" type="integer" column="thumb_size" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="optLabel" type="string" column="opt_label" length="32" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">right/wrong</option>
      </options>
    </field>
    <field name="customTrue" type="string" column="custom_true" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customFalse" type="string" column="custom_false" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="scorePartsol" type="boolean" column="score_partsol" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="feedbackSetting" type="integer" column="feedback_setting" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
