<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EcsCmapRule" table="ecs_cmap_rule">
    <id name="rid" type="integer" column="rid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sid" type="integer" column="sid" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="mid" type="integer" column="mid" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="attribute" type="string" column="attribute" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="refId" type="integer" column="ref_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isFilter" type="boolean" column="is_filter" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="filter" type="string" column="filter" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createSubdir" type="boolean" column="create_subdir" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="subdirType" type="boolean" column="subdir_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="directory" type="string" column="directory" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
