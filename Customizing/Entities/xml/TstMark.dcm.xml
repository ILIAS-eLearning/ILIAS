<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TstMark" table="tst_mark">
    <indexes>
      <index name="i1_idx" columns="test_fi"/>
    </indexes>
    <id name="markId" type="integer" column="mark_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="testFi" type="integer" column="test_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="shortName" type="string" column="short_name" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="officialName" type="string" column="official_name" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="minimumLevel" type="float" column="minimum_level" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="passed" type="string" column="passed" length="1" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
