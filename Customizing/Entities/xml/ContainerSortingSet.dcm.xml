<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ContainerSortingSet" table="container_sorting_set">
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sortMode" type="boolean" column="sort_mode" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="sortDirection" type="boolean" column="sort_direction" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="newItemsPosition" type="boolean" column="new_items_position" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="newItemsOrder" type="boolean" column="new_items_order" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
