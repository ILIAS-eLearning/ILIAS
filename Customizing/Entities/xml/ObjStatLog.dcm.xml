<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ObjStatLog" table="obj_stat_log">
    <indexes>
      <index name="i1_idx" columns="tstamp"/>
    </indexes>
    <id name="logId" type="integer" column="log_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="objId" type="integer" column="obj_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="objType" type="string" column="obj_type" length="10" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="yyyy" type="smallint" column="yyyy" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mm" type="boolean" column="mm" nullable="true"/>
    <field name="dd" type="boolean" column="dd" nullable="true"/>
    <field name="hh" type="boolean" column="hh" nullable="true"/>
    <field name="readCount" type="integer" column="read_count" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="childsReadCount" type="integer" column="childs_read_count" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="spentSeconds" type="integer" column="spent_seconds" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="childsSpentSeconds" type="integer" column="childs_spent_seconds" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
