<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdvMdValuesText" table="adv_md_values_text">
    <indexes>
      <index name="i1_idx" columns="obj_id"/>
    </indexes>
    <id name="objId" type="integer" column="obj_id"/>
    <id name="fieldId" type="integer" column="field_id"/>
    <id name="subType" type="string" column="sub_type" length="10"/>
    <id name="subId" type="integer" column="sub_id"/>
    <field name="value" type="string" column="value" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="disabled" type="boolean" column="disabled" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
