<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TstTestRndQst" table="tst_test_rnd_qst">
    <indexes>
      <index name="i1_idx" columns="question_fi"/>
      <index name="i3_idx" columns="pass"/>
      <index name="i2_idx" columns="active_fi"/>
    </indexes>
    <id name="testRandomQuestionId" type="integer" column="test_random_question_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="activeFi" type="integer" column="active_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="questionFi" type="integer" column="question_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="sequence" type="smallint" column="sequence" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pass" type="integer" column="pass" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="srcPoolDefFi" type="integer" column="src_pool_def_fi" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
