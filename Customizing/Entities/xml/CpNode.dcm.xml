<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CpNode" table="cp_node">
    <indexes>
      <index name="i2_idx" columns="nodename"/>
      <index name="i3_idx" columns="slm_id"/>
    </indexes>
    <id name="cpNodeId" type="integer" column="cp_node_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nodename" type="string" column="nodename" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="slmId" type="integer" column="slm_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
