<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LmData" table="lm_data">
    <indexes>
      <index name="i1_idx" columns="lm_id"/>
      <index name="im_idx" columns="import_id"/>
      <index name="i2_idx" columns="type"/>
    </indexes>
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="2" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="lmId" type="integer" column="lm_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="importId" type="string" column="import_id" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="publicAccess" type="string" column="public_access" length="1" nullable="true">
      <options>
        <option name="fixed">1</option>
        <option name="default">n</option>
      </options>
    </field>
    <field name="createDate" type="datetime" column="create_date" nullable="true"/>
    <field name="lastUpdate" type="datetime" column="last_update" nullable="true"/>
    <field name="active" type="string" column="active" length="1" nullable="true">
      <options>
        <option name="fixed">1</option>
        <option name="default">y</option>
      </options>
    </field>
    <field name="layout" type="string" column="layout" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="shortTitle" type="string" column="short_title" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
