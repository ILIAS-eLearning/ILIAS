<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CtrlClassfile" table="ctrl_classfile">
    <indexes>
      <index name="i1_idx" columns="cid"/>
    </indexes>
    <id name="class" type="string" column="class" length="100">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="filename" type="string" column="filename" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="compPrefix" type="string" column="comp_prefix" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pluginPath" type="string" column="plugin_path" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cid" type="string" column="cid" length="4" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
