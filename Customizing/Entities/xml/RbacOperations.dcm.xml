<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RbacOperations" table="rbac_operations">
    <indexes>
      <index name="i1_idx" columns="operation"/>
    </indexes>
    <id name="opsId" type="integer" column="ops_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="operation" type="string" column="operation" length="32" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="class" type="string" column="class" length="16" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="opOrder" type="smallint" column="op_order" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
