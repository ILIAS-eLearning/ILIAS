<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ScItem" table="sc_item">
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="importId" type="string" column="import_id" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="identifierref" type="string" column="identifierref" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isvisible" type="string" column="isvisible" length="6" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="parameters" type="string" column="parameters" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prereqType" type="string" column="prereq_type" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prerequisites" type="string" column="prerequisites" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="maxtimeallowed" type="string" column="maxtimeallowed" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="timelimitaction" type="string" column="timelimitaction" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="datafromlms" type="text" column="datafromlms" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="masteryscore" type="string" column="masteryscore" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
