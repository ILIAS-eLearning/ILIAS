<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TstSklThresholds" table="tst_skl_thresholds">
    <id name="testFi" type="integer" column="test_fi"/>
    <id name="skillBaseFi" type="integer" column="skill_base_fi"/>
    <id name="skillTrefFi" type="integer" column="skill_tref_fi"/>
    <id name="skillLevelFi" type="integer" column="skill_level_fi"/>
    <field name="threshold" type="integer" column="threshold" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
