<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TstResultCache" table="tst_result_cache">
    <id name="activeFi" type="integer" column="active_fi">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="pass" type="integer" column="pass" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="maxPoints" type="float" column="max_points" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="reachedPoints" type="float" column="reached_points" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="markShort" type="string" column="mark_short" length="256" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="markOfficial" type="string" column="mark_official" length="256" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="passed" type="integer" column="passed" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="failed" type="integer" column="failed" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="hintCount" type="integer" column="hint_count" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="hintPoints" type="float" column="hint_points" precision="10" scale="0" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="obligationsAnswered" type="boolean" column="obligations_answered" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="passedOnce" type="boolean" column="passed_once" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
