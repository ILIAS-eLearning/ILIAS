<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AiccCourse" table="aicc_course">
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="courseCreator" type="string" column="course_creator" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="courseId" type="string" column="course_id" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="courseSystem" type="string" column="course_system" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="courseTitle" type="string" column="course_title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cLevel" type="string" column="c_level" length="5" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="maxFieldsCst" type="integer" column="max_fields_cst" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="maxFieldsOrt" type="integer" column="max_fields_ort" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="totalAus" type="integer" column="total_aus" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="totalBlocks" type="integer" column="total_blocks" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="totalComplexObj" type="integer" column="total_complex_obj" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="totalObjectives" type="integer" column="total_objectives" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="version" type="string" column="version" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="maxNormal" type="boolean" column="max_normal" nullable="true"/>
    <field name="description" type="text" column="description" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
