<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MapArea" table="map_area">
    <indexes>
      <index name="lt_idx" columns="link_type,target"/>
    </indexes>
    <id name="itemId" type="integer" column="item_id"/>
    <id name="nr" type="integer" column="nr"/>
    <field name="shape" type="string" column="shape" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="coords" type="string" column="coords" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="linkType" type="string" column="link_type" length="3" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="href" type="string" column="href" length="800" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="target" type="string" column="target" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="targetFrame" type="string" column="target_frame" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="highlightMode" type="string" column="highlight_mode" length="8" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="highlightClass" type="string" column="highlight_class" length="8" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
