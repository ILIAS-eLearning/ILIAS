<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LsoActivation" table="lso_activation">
    <id name="refId" type="integer" column="ref_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="online" type="boolean" column="online" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="effectiveOnline" type="boolean" column="effective_online" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="activationStartTs" type="integer" column="activation_start_ts" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="activationEndTs" type="integer" column="activation_end_ts" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
