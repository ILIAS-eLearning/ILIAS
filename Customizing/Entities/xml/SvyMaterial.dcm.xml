<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SvyMaterial" table="svy_material">
    <indexes>
      <index name="i1_idx" columns="question_fi"/>
    </indexes>
    <id name="materialId" type="integer" column="material_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="questionFi" type="integer" column="question_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="internalLink" type="string" column="internal_link" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="importId" type="string" column="import_id" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="materialTitle" type="string" column="material_title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="textMaterial" type="string" column="text_material" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="externalLink" type="string" column="external_link" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fileMaterial" type="string" column="file_material" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="materialType" type="integer" column="material_type" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
