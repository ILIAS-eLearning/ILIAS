<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MailTemplate" table="mail_template">
    <id name="lang" type="string" column="lang" length="5"/>
    <id name="type" type="string" column="type" length="4"/>
    <field name="subject" type="string" column="subject" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="body" type="text" column="body" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="salF" type="string" column="sal_f" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="salM" type="string" column="sal_m" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="salG" type="string" column="sal_g" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="attFile" type="string" column="att_file" length="400" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
