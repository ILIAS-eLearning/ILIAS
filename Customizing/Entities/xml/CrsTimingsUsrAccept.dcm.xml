<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CrsTimingsUsrAccept" table="crs_timings_usr_accept">
    <id name="crsId" type="integer" column="crs_id"/>
    <id name="usrId" type="integer" column="usr_id"/>
    <field name="accept" type="boolean" column="accept" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="remark" type="string" column="remark" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="visible" type="boolean" column="visible" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
