<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CalEntries" table="cal_entries">
    <indexes>
      <index name="i1_idx" columns="last_update"/>
      <index name="i2_idx" columns="context_id"/>
    </indexes>
    <id name="calId" type="integer" column="cal_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lastUpdate" type="datetime" column="last_update" nullable="true"/>
    <field name="title" type="string" column="title" length="128" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="subtitle" type="string" column="subtitle" length="64" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="description" type="string" column="description" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="location" type="string" column="location" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fullday" type="boolean" column="fullday" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="starta" type="datetime" column="starta" nullable="true"/>
    <field name="enda" type="datetime" column="enda" nullable="true"/>
    <field name="informations" type="string" column="informations" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="autoGenerated" type="boolean" column="auto_generated" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="contextId" type="integer" column="context_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="translationType" type="boolean" column="translation_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isMilestone" type="boolean" column="is_milestone" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="completion" type="integer" column="completion" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="notification" type="boolean" column="notification" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="contextInfo" type="string" column="context_info" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
