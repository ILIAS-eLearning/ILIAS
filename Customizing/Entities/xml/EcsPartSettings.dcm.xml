<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EcsPartSettings" table="ecs_part_settings">
    <id name="sid" type="integer" column="sid"/>
    <id name="mid" type="integer" column="mid"/>
    <field name="export" type="boolean" column="export" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="import" type="boolean" column="import" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="importType" type="boolean" column="import_type" nullable="true"/>
    <field name="title" type="string" column="title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cname" type="string" column="cname" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="token" type="boolean" column="token" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="exportTypes" type="string" column="export_types" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="importTypes" type="string" column="import_types" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dtoken" type="boolean" column="dtoken" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
