<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LdapRgMapping" table="ldap_rg_mapping">
    <id name="mappingId" type="integer" column="mapping_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="serverId" type="integer" column="server_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="url" type="string" column="url" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dn" type="string" column="dn" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="memberAttribute" type="string" column="member_attribute" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="memberIsdn" type="boolean" column="member_isdn" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="role" type="integer" column="role" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="mappingInfo" type="string" column="mapping_info" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mappingInfoType" type="boolean" column="mapping_info_type" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
