<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlTag" table="il_tag">
    <indexes>
      <index name="i3_idx" columns="user_id"/>
      <index name="i1_idx" columns="obj_id,obj_type,sub_obj_id,sub_obj_type"/>
      <index name="i2_idx" columns="tag"/>
    </indexes>
    <id name="objId" type="integer" column="obj_id"/>
    <id name="objType" type="string" column="obj_type" length="10"/>
    <id name="subObjId" type="integer" column="sub_obj_id"/>
    <id name="subObjType" type="string" column="sub_obj_type" length="10"/>
    <id name="userId" type="integer" column="user_id"/>
    <id name="tag" type="string" column="tag" length="100"/>
    <field name="isOffline" type="boolean" column="is_offline" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
