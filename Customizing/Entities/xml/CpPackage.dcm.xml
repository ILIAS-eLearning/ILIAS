<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CpPackage" table="cp_package">
    <indexes>
      <index name="i1_idx" columns="c_identifier"/>
    </indexes>
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="created" type="string" column="created" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cIdentifier" type="string" column="c_identifier" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="jsdata" type="text" column="jsdata" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="modified" type="string" column="modified" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="persistprevattempts" type="integer" column="persistprevattempts" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="cSettings" type="string" column="c_settings" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="xmldata" type="text" column="xmldata" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="activitytree" type="text" column="activitytree" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="globalToSystem" type="boolean" column="global_to_system" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="sharedDataGlobalToSystem" type="boolean" column="shared_data_global_to_system" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
