<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlDclTableview" table="il_dcl_tableview">
    <indexes>
      <index name="t1_idx" columns="table_id"/>
    </indexes>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tableId" type="bigint" column="table_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="128" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="roles" type="text" column="roles" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tableviewOrder" type="bigint" column="tableview_order" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
