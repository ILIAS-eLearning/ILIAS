<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MepItem" table="mep_item">
    <indexes>
      <index name="ft_idx" columns="foreign_id,type"/>
    </indexes>
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="type" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="foreignId" type="integer" column="foreign_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="importId" type="string" column="import_id" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
