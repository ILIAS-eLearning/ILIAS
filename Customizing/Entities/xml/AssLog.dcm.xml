<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AssLog" table="ass_log">
    <indexes>
      <index name="i1_idx" columns="user_fi,obj_fi"/>
      <index name="i2_idx" columns="obj_fi"/>
    </indexes>
    <id name="assLogId" type="integer" column="ass_log_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userFi" type="integer" column="user_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="objFi" type="integer" column="obj_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="logtext" type="string" column="logtext" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="questionFi" type="integer" column="question_fi" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="originalFi" type="integer" column="original_fi" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="refId" type="integer" column="ref_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="testOnly" type="string" column="test_only" length="1" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
