<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CpManifest" table="cp_manifest">
    <indexes>
      <index name="i1_idx" columns="id"/>
    </indexes>
    <id name="cpNodeId" type="integer" column="cp_node_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="base" type="string" column="base" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="defaultorganization" type="string" column="defaultorganization" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="id" type="string" column="id" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="uri" type="string" column="uri" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="version" type="string" column="version" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
