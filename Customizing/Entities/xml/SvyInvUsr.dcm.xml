<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SvyInvUsr" table="svy_inv_usr">
    <indexes>
      <index name="i1_idx" columns="survey_fi"/>
      <index name="i2_idx" columns="user_fi"/>
    </indexes>
    <id name="invitedUserId" type="integer" column="invited_user_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="surveyFi" type="integer" column="survey_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="userFi" type="integer" column="user_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
