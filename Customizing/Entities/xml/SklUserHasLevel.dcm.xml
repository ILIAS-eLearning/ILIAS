<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SklUserHasLevel" table="skl_user_has_level">
    <id name="levelId" type="integer" column="level_id"/>
    <id name="userId" type="integer" column="user_id"/>
    <id name="triggerObjId" type="integer" column="trigger_obj_id"/>
    <id name="trefId" type="integer" column="tref_id"/>
    <id name="selfEval" type="boolean" column="self_eval"/>
    <field name="statusDate" type="datetime" column="status_date" nullable="false">
      <options>
        <option name="default">1970-01-01 00:00:00</option>
      </options>
    </field>
    <field name="skillId" type="integer" column="skill_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="triggerRefId" type="integer" column="trigger_ref_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="triggerTitle" type="string" column="trigger_title" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="triggerObjType" type="string" column="trigger_obj_type" length="4" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">crs</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
