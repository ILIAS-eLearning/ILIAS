<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UsrSearch" table="usr_search">
    <id name="usrId" type="integer" column="usr_id"/>
    <id name="searchType" type="boolean" column="search_type"/>
    <field name="searchResult" type="text" column="search_result" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="checked" type="text" column="checked" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="failed" type="text" column="failed" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="page" type="boolean" column="page" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="query" type="text" column="query" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="root" type="integer" column="root" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="itemFilter" type="string" column="item_filter" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mimeFilter" type="string" column="mime_filter" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creationFilter" type="string" column="creation_filter" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
