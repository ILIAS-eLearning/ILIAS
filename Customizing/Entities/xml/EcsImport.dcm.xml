<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EcsImport" table="ecs_import">
    <id name="objId" type="integer" column="obj_id"/>
    <id name="serverId" type="integer" column="server_id"/>
    <field name="mid" type="integer" column="mid" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="subId" type="string" column="sub_id" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ecsId" type="integer" column="ecs_id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="contentId" type="string" column="content_id" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="econtentId" type="string" column="econtent_id" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
