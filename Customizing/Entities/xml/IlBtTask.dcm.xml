<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlBtTask" table="il_bt_task">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="type" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="classPath" type="string" column="class_path" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="className" type="string" column="class_name" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bucketId" type="bigint" column="bucket_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
