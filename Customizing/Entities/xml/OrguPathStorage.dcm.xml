<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OrguPathStorage" table="orgu_path_storage">
    <id name="refId" type="bigint" column="ref_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="objId" type="bigint" column="obj_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="path" type="text" column="path" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
