<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EcsCmsData" table="ecs_cms_data">
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="serverId" type="integer" column="server_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mid" type="integer" column="mid" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="treeId" type="integer" column="tree_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="term" type="string" column="term" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="smallint" column="status" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="deleted" type="boolean" column="deleted" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="cmsId" type="string" column="cms_id" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
