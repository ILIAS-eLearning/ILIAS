<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RegRegistrationCodes" table="reg_registration_codes">
    <indexes>
      <index name="i1_idx" columns="code"/>
    </indexes>
    <id name="codeId" type="integer" column="code_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="code" type="string" column="code" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="role" type="integer" column="role" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="generatedOn" type="integer" column="generated_on" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="used" type="integer" column="used" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="roleLocal" type="string" column="role_local" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="alimit" type="string" column="alimit" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="alimitdt" type="string" column="alimitdt" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="regEnabled" type="boolean" column="reg_enabled" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="extEnabled" type="boolean" column="ext_enabled" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
