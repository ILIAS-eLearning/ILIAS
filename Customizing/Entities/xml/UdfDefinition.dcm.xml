<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UdfDefinition" table="udf_definition">
    <id name="fieldId" type="integer" column="field_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fieldName" type="string" column="field_name" length="255" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="fieldType" type="boolean" column="field_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fieldValues" type="text" column="field_values" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="visible" type="boolean" column="visible" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="changeable" type="boolean" column="changeable" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="required" type="boolean" column="required" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="searchable" type="boolean" column="searchable" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="export" type="boolean" column="export" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="courseExport" type="boolean" column="course_export" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="registrationVisible" type="boolean" column="registration_visible" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="visibleLua" type="boolean" column="visible_lua" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="changeableLua" type="boolean" column="changeable_lua" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="groupExport" type="boolean" column="group_export" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="certificate" type="boolean" column="certificate" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
