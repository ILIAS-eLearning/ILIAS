<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplQstOrdering" table="qpl_qst_ordering">
    <id name="questionFi" type="integer" column="question_fi">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="orderingType" type="string" column="ordering_type" length="1" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="thumbGeometry" type="integer" column="thumb_geometry" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">100</option>
      </options>
    </field>
    <field name="elementHeight" type="integer" column="element_height" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="scoringType" type="integer" column="scoring_type" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="reducedPoints" type="float" column="reduced_points" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
