<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserActionActivation" table="user_action_activation">
    <id name="contextComp" type="string" column="context_comp" length="30"/>
    <id name="contextId" type="string" column="context_id" length="30"/>
    <id name="actionComp" type="string" column="action_comp" length="30"/>
    <id name="actionType" type="string" column="action_type" length="30"/>
    <field name="active" type="boolean" column="active" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
