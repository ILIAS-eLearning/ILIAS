<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ScResource" table="sc_resource">
    <indexes>
      <index name="i1_idx" columns="import_id"/>
    </indexes>
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="importId" type="string" column="import_id" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="resourcetype" type="string" column="resourcetype" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="scormtype" type="string" column="scormtype" length="6" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="href" type="string" column="href" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="xmlBase" type="string" column="xml_base" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
