<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlMediaCastData" table="il_media_cast_data">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="isOnline" type="boolean" column="is_online" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="publicFiles" type="boolean" column="public_files" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="downloadable" type="boolean" column="downloadable" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="defAccess" type="boolean" column="def_access" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="sortmode" type="boolean" column="sortmode" nullable="true">
      <options>
        <option name="default">3</option>
      </options>
    </field>
    <field name="viewmode" type="string" column="viewmode" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
