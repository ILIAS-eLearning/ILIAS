<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UtLpCollections" table="ut_lp_collections">
    <id name="objId" type="integer" column="obj_id"/>
    <id name="itemId" type="integer" column="item_id"/>
    <field name="groupingId" type="integer" column="grouping_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="numObligatory" type="integer" column="num_obligatory" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="lpmode" type="boolean" column="lpmode" nullable="true">
      <options>
        <option name="default">5</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
