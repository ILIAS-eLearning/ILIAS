<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlMmCustomItems" table="il_mm_custom_items">
    <id name="identifier" type="string" column="identifier" length="255">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="type" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="action" type="string" column="action" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="topItem" type="boolean" column="top_item" nullable="true"/>
    <field name="defaultTitle" type="string" column="default_title" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
