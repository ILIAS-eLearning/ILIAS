<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LastVisited" table="last_visited">
    <id name="userId" type="integer" column="user_id"/>
    <id name="nr" type="integer" column="nr"/>
    <field name="refId" type="integer" column="ref_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="type" type="string" column="type" length="10" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="subObjId" type="string" column="sub_obj_id" length="40" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gotoLink" type="string" column="goto_link" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
