<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UsrSessionStatsRaw" table="usr_session_stats_raw">
    <id name="sessionId" type="string" column="session_id" length="80">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="smallint" column="type" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="startTime" type="integer" column="start_time" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="endTime" type="integer" column="end_time" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="endContext" type="smallint" column="end_context" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
