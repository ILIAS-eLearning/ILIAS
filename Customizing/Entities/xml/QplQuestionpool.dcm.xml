<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QplQuestionpool" table="qpl_questionpool">
    <indexes>
      <index name="i1_idx" columns="obj_fi"/>
    </indexes>
    <id name="idQuestionpool" type="integer" column="id_questionpool">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="objFi" type="integer" column="obj_fi" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isonline" type="string" column="isonline" length="1" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="questioncount" type="integer" column="questioncount" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tstamp" type="integer" column="tstamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="showTaxonomies" type="boolean" column="show_taxonomies" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="navTaxonomy" type="integer" column="nav_taxonomy" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="skillService" type="boolean" column="skill_service" nullable="true"/>
  </entity>
</doctrine-mapping>
