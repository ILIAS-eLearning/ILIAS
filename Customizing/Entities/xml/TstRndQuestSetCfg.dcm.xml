<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TstRndQuestSetCfg" table="tst_rnd_quest_set_cfg">
    <id name="testFi" type="integer" column="test_fi">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="reqPoolsHomoScored" type="boolean" column="req_pools_homo_scored" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="questAmountCfgMode" type="string" column="quest_amount_cfg_mode" length="16" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="questAmountPerTest" type="integer" column="quest_amount_per_test" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="questSyncTimestamp" type="integer" column="quest_sync_timestamp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
