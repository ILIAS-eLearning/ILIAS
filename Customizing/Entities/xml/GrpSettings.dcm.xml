<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GrpSettings" table="grp_settings">
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="information" type="string" column="information" length="4000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="grpType" type="boolean" column="grp_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="registrationType" type="boolean" column="registration_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="registrationEnabled" type="boolean" column="registration_enabled" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="registrationUnlimited" type="boolean" column="registration_unlimited" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="registrationStart" type="datetime" column="registration_start" nullable="true"/>
    <field name="registrationEnd" type="datetime" column="registration_end" nullable="true"/>
    <field name="registrationPassword" type="string" column="registration_password" length="32" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="registrationMemLimit" type="boolean" column="registration_mem_limit" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="registrationMaxMembers" type="integer" column="registration_max_members" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="waitingList" type="boolean" column="waiting_list" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="latitude" type="string" column="latitude" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="longitude" type="string" column="longitude" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="locationZoom" type="integer" column="location_zoom" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="enablemap" type="boolean" column="enablemap" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="regAcEnabled" type="boolean" column="reg_ac_enabled" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="regAc" type="string" column="reg_ac" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="viewMode" type="boolean" column="view_mode" nullable="false">
      <options>
        <option name="default">6</option>
      </options>
    </field>
    <field name="mailMembersType" type="boolean" column="mail_members_type" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="registrationMinMembers" type="smallint" column="registration_min_members" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="leaveEnd" type="integer" column="leave_end" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="autoWait" type="boolean" column="auto_wait" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="showMembers" type="boolean" column="show_members" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="grpStart" type="integer" column="grp_start" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="grpEnd" type="integer" column="grp_end" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
