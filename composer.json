		"psr-4" : {
			"ILIAS\\Tests\\" : "./tests",
			"ILIAS\\" : "./src"
		},
		"classmap": [
			"./Services/",
			"./Modules",
			"./libs/ilias"
		],
		"exclude-from-classmap": [
			"./Services/Migration",
			"./*/*/lib", "./*/*/test",
			"./Services/CAS/phpcas"
		]
	},
