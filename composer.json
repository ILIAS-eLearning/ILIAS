{
	"name": "ilias/ilias",
	"description": "ILIAS",
	"license": "GPL v3",
	"authors": [],
	"config": {
		"optimize-autoloader": true,
		"vendor-dir": "./libs/composer/vendor",
		"allow-plugins": {
			"simplesamlphp/composer-module-installer": true,
			"cweagans/composer-patches": true,
			"captainhook/plugin-composer": true
		}
	},
	"scripts": {
		"post-autoload-dump": [
			"@php setup/cli.php build-artifacts --yes"
		],
		"post-install-cmd": [
			"@php libs/composer/rmdirs.php"
		],
		"post-update-cmd": [
			"@php libs/composer/rmdirs.php"
		],
		"test-php-all": [
			"./libs/composer/vendor/bin/phpunit -c ./Services/PHPUnit/config/PhpUnitConfig.xml --colors=always --disallow-todo-tests"
		],
		"test-php": [
			"./CI/PHPUnit/run_tests.sh"
		],
		"rector": [
			"./libs/composer/vendor/bin/rector process --config ./CI/Rector/basic_rector.php --no-diffs"
		]
	},
	"require": {
		"php": ">=7.4 <8.2",
		"ext-gd": "*",
		"ext-dom": "*",
		"ext-xsl": "*",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-simplexml": "*",
		"ext-libxml": "*",
		"cweagans/composer-patches": "^1.7",
		"filp/whoops": "^2.1.13",
		"monolog/monolog": "^2.3.5",
		"technosophos/libris": "^2.0.2",
		"ezyang/htmlpurifier": "4.14.0",
		"phpmailer/phpmailer": "^6.5",
		"geshi/geshi": "^1.0.9.1",
		"tecnickcom/tcpdf": "^6.3",
		"pimple/pimple" : "^3.0",
		"symfony/yaml": "^5.3",
		"guzzlehttp/psr7": "1.8.5",
		"psr/http-message": "^1.0",
		"dflydev/fig-cookies": "^1.0",
		"ralouphie/getallheaders": "^2.0",
		"league/flysystem": "^1.0",
		"james-heinrich/getid3": "^1.9",
		"phpoffice/phpspreadsheet": "^1.18.0",
		"jumbojett/openid-connect-php": "^0.9.5",
		"sabre/dav": "^4.1",
		"symfony/console" : "^5.0",
		"slim/slim": "^3.11",
		"ramsey/uuid": "^3.8",
		"enshrined/svg-sanitize": "^0.15.0",
		"guzzlehttp/guzzle": "^6.3",
		"simplesamlphp/simplesamlphp": "^1.19.4",
		"seld/jsonlint": "^1.8",
		"ext-xml": "*",
		"jasig/phpcas": "^1.4"
	},
	"require-dev": {
		"mikey179/vfsstream": "^1.6",
		"phpunit/phpunit": "^9.5",
		"mockery/mockery": "^1.4",
		"friendsofphp/php-cs-fixer": "^3.5",
		"sebastian/environment": "^5.0",
		"captainhook/captainhook": "^5.10",
		"captainhook/plugin-composer": "^5.3",
		"rector/rector": "^0.12.12",
		"phpstan/phpstan": "^1.6"
	},
	"autoload": {
		"psr-4" : {
			"ILIAS\\Tests\\" : "./tests",
			"ILIAS\\" : "./src",
			"ILIAS\\CI\\" : "./CI"
		},
		"classmap": [
			"./Customizing/global/plugins",
			"./Services/",
			"./Modules",
			"./libs/ilias",
			"./CI/Rector",
			"./webservice/soap",
			"./setup/classes"
		],
		"exclude-from-classmap": [
			"./Services/Migration",
			"./*/*/lib", "./*/*/test",
			"./Customizing/**/vendor"
		]
	},
