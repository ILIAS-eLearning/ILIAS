<?xml version="1.0" encoding="UTF-8"?>
<ilLayout>
	<ilFrame name="maincontent">
		<StructureObject template="tpl.page_new.html" template_location="module">
			<ilPage/>
			<ilLMNavigation/>
			<ilLocator/>
			<ilLMMenu />
			<ilMainMenu/>
			<ilLMHead />
			<ilLMSubMenu />
			<ilLMNotes />
		</StructureObject>
		<PageObject template="tpl.page_new.html" template_location="module">
			<ilPage/>
			<ilLMNavigation/>
			<ilLocator/>
			<ilLMMenu />
			<ilMainMenu/>
			<ilLMHead />
			<ilLMSubMenu />
			<ilLMNotes />
		</PageObject>
	</ilFrame>
	<ilFrame name="right_top" template="tpl.blank.html" template_location="module">
		<PageObject template="tpl.page.html" template_location="module">
			<ilPage/>
		</PageObject>
		<MediaObject template="tpl.fullscreen.html" template_location="module">
			<ilMedia/>
		</MediaObject>
	</ilFrame>
	<ilFrame name="right_bottom" template="tpl.blank.html" template_location="module">
		<GlossaryItem template="tpl.glossary_term_output.html" template_location="module">
			<ilGlossary/>
		</GlossaryItem>
	</ilFrame>
	<ilLinkTargets>
		<LinkTarget Type="New" Frame="_blank"/>
		<LinkTarget Type="FAQ" Frame="right_top" OnClick="return il.LearningModule.showContentFrame(event, 'right_top');"/>
		<LinkTarget Type="Glossary" Frame="right_bottom" OnClick="return il.LearningModule.showContentFrame(event, 'right_bottom');"/>
		<LinkTarget Type="Media" Frame="right_top" OnClick="return il.LearningModule.showContentFrame(event, 'right_top');"/>
	</ilLinkTargets>
</ilLayout>
