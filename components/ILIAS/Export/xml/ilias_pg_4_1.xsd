		<element ref='t:Code'/>
		<element ref='t:ExtLink'/>
		<element ref='t:IntLink'/>
		<element ref='t:SimpleBulletList'/>
		<element ref='t:SimpleNumberedList'/>
	</choice>
</complexType>

<!-- CommentText -->
<complexType name='CommentText' mixed='true'>
