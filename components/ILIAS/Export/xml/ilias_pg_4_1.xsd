    <element ref='t:Emph'/>
    <element ref='t:Accent'/>
    <element ref='t:Important'/>
    <element ref='t:Quotation'/>
    <element ref='t:Code'/>
 	</choice>
</complexType>

<!-- LinkText -->
<complexType name='LinkText' mixed='true'>
   <choice minOccurs='0' maxOccurs='unbounded'>
    <element ref='t:br'/>
    <element ref='t:Comment'/>
    <element ref='t:Emph'/>
    <element ref='t:Strong'/>
    <element ref='t:Quotation'/>
    <element ref='t:Code'/>
 	</choice>
</complexType>


<!-- SimpleText -->
<complexType name='SimpleText' mixed='true'>
   <choice minOccurs='0' maxOccurs='unbounded'>
    <element ref='t:br'/>
    <element ref='t:Emph'/>
    <element ref='t:Strong'/>
    <element ref='t:Quotation'/>
    <element ref='t:Code'/>
    <element ref='t:Anchor'/>
    <element ref='t:ExtLink'/>
    <element ref='t:IntLink'/>
 	</choice>
</complexType>


<!-- FootnoteText -->
<complexType name='FootnoteText' mixed='true'>
   <choice minOccurs='0' maxOccurs='unbounded'>
    <element ref='t:br'/>
    <element ref='t:Comment'/>
    <element ref='t:Emph'/>
    <element ref='t:Accent'/>
    <element ref='t:Strong'/>
    <element ref='t:Anchor'/>
    <element ref='t:ExtLink'/>
    <element ref='t:IntLink'/>
    <element ref='t:Important'/>
    <element ref='t:Quotation'/>
    <element ref='t:Code'/>
   </choice>
</complexType>


<!-- TitleText -->
<complexType name='TitleText' mixed='true'>
   <choice minOccurs='0' maxOccurs='unbounded'>
    <element ref='t:br'/>
    <element ref='t:Emph'/>
    <element ref='t:Strong'/>
    <element ref='t:Quotation'/>
    <element ref='t:Code'/>
   </choice>
</complexType>

<!-- PageObject -->
 <element name='PageObject'>
  <complexType>
   <sequence>
    <element ref='t:PageContent' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:Layout' minOccurs='0' maxOccurs='1'/>
   </sequence>
  </complexType>
 </element>

<!-- PageContent -->
 <element name='PageContent'>
  <complexType>
   <sequence>
    <choice>
     <element ref='t:Paragraph'/>
     <element ref='t:MediaObject'/>
     <element ref='t:Table'/>
     <element ref='t:List'/>
     <element ref='t:FileList'/>
     <element ref='t:Question'/>
     <element ref='t:PlaceHolder'/>
     <element ref='t:Section'/>
     <element ref='t:Resources'/>
     <element ref='t:Map'/>
     <element ref='t:Plugged'/>
     <element ref='t:Tabs'/>
     <element ref='t:ContentInclude'/>
    </choice>
    <element ref='t:BibItemIdentifier' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='PCID' type='string' use='optional'/>
   <attribute name='Enabled' use='optional'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='True'/>
      <enumeration value='False'/>
     </restriction>
    </simpleType>
   </attribute>
  </complexType>
 </element>

 <!-- BibItemIdentifier -->
 <element name='BibItemIdentifier'>
  <complexType>
   <attribute name='Catalog' type='string' use='optional'/>
   <attribute name='Entry' type='string' use='required'/>
  </complexType>
 </element>

 <!-- PageTurn -->
 <element name='PageTurn'>
  <complexType>
   <sequence>
    <element ref='t:BibItemIdentifier'/>
   </sequence>
   <attribute name='NumberingType' use='required'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='Roman'/>
      <enumeration value='Arabic'/>
      <enumeration value='Alphanumeric'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='Number' type='string' use='optional'/>
  </complexType>
 </element>

 <!-- Identifier -->
 <element name='Identifier'>
  <complexType>
   <attribute name='Catalog' type='string' use='optional'/>
   <attribute name='Entry' type='string' use='required'/>
  </complexType>
 </element>

 <!-- Title -->
 <element name='Title'>
  <complexType mixed='true'>
   <complexContent>
    <extension base="t:TitleText">
     <attribute name='Language' type='t:LanguageCode' use='required' />
    </extension>
   </complexContent>
  </complexType>
 </element>

 <!-- MediaObject -->
 <element name='MediaObject'>
  <complexType>
    <sequence>
     <element ref='t:MediaAlias'/>
     <element ref='t:MediaAliasItem' maxOccurs='unbounded'/>
    </sequence>
  </complexType>
 </element>

 <!-- MediaObject -->
 <element name='MediaAlias'>
  <complexType>
   <attribute name='OriginId' type='string' use='required'/>
   <attribute name='Class' type='string' use='optional'/>
  </complexType>
 </element>

 <!-- MediaAliasItem -->
 <element name='MediaAliasItem'>
  <complexType>
   <sequence>
    <element ref='t:Layout' minOccurs='0' maxOccurs='1'/>
    <element ref='t:Caption' minOccurs='0' maxOccurs='1'/>
    <element ref='t:TextRepresentation' minOccurs='0' maxOccurs='1'/>
    <element ref='t:Parameter' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:MapArea' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='Purpose' use='required'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='Standard'/>
      <enumeration value='Fullscreen'/>
      <enumeration value='Additional'/>
     </restriction>
    </simpleType>
   </attribute>
  </complexType>
 </element>

 <!-- MapArea -->
 <element name='MapArea'>
  <complexType>
   <choice>
    <element ref='t:ExtLink'/>
    <element ref='t:IntLink'/>
   </choice>
   <attribute name='Shape' use='required'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='Rect'/>
      <enumeration value='Circle'/>
      <enumeration value='Poly'/>
      <enumeration value='WholePicture'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='Coords' type='string' use='optional'/>
  </complexType>
 </element>

 <!-- Text Representation -->
 <element name='TextRepresentation'>
  <complexType mixed='true'>
  </complexType>
 </element>

 <!-- Format -->
 <element name='Format'>
  <complexType mixed='true'>
  </complexType>
 </element>

 <!-- Location -->
 <element name='Location'>
  <complexType mixed='true'>
   <attribute name='Type' use='required'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='LocalFile'/>
      <enumeration value='Reference'/>
     </restriction>
    </simpleType>
   </attribute>
  </complexType>
 </element>

 <!-- Paragraph -->
 <element name='Paragraph'>
  <complexType mixed='true'>
   <complexContent>
    <extension base="t:Text">
	   <attribute name='Language' type='t:LanguageCode' use='required' />
	   <attribute name='Characteristic' type='string' use='optional'/>
	   <attribute name='SubCharacteristic' type='string' use='optional'/>
	   <attribute name='AutoIndent' use='optional'>
		<simpleType>
		 <restriction base='string'>
		  <enumeration value='y'/>
		  <enumeration value='n'/>
		 </restriction>
		</simpleType>
	   </attribute>
	   <attribute name='ShowLineNumbers' use='optional'>
		<simpleType>
		 <restriction base='string'>
		  <enumeration value='y'/>
		  <enumeration value='n'/>
		 </restriction>
		</simpleType>
	   </attribute>
	   <attribute name='DownloadTitle' type='string' use='optional'/>
    </extension>
   </complexContent>
  </complexType>
 </element>

 <!-- br -->
 <element name='br'>
  <complexType/>
 </element>

 <!-- Comment -->
 <element name='Comment' type='t:CommentText' />

 <!-- Emph -->
 <element name='Emph' type='t:EmphText' />

 <!-- Footnote -->
 <element name='Footnote' type='t:FootnoteText' />

 <!-- Strong -->
 <element name='Strong' type='t:StrongText' />

 <!-- Accent -->
 <element name='Accent' type='t:AccentText' />

 <!-- Important -->
 <element name='Important' type='t:AccentText' />

 <!-- Quotation -->
 <element name='Quotation'>
  <complexType mixed='true'>
   <complexContent>
    <extension base="t:Text">
		<attribute name='Language' type='t:LanguageCode' use='required' />
		<attribute name='Reference' type='IDREF' use='optional'/>
	</extension>
   </complexContent>
  </complexType>
 </element>

 <!-- Keyw -->
 <element name='Keyw' type='t:KeywText' />

 <!-- Code -->
 <element name='Code'>
  <complexType mixed='true'>
   <sequence minOccurs='0' maxOccurs='unbounded'>
    <element ref='t:br'/>
   </sequence>
   <attribute name='Id' type='ID' use='optional'/>
   <attribute name='ProgrammingLanguage' type='string' use='optional'/>
  </complexType>
 </element>

 <!-- Anchor -->
 <element name='Anchor'>
  <complexType mixed='true'>
   <complexContent>
    <extension base="t:Text">
		<attribute name='Name' type='string' use='optional'/>
	</extension>
   </complexContent>
  </complexType>
 </element>

 <!-- List -->
 <element name='List'>
  <complexType>
   <sequence>
    <element ref='t:Title' minOccurs='0' maxOccurs='1'/>
    <element ref='t:ListItem' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='Id' type='ID' use='optional'/>
   <attribute name='Type' use='required'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='Ordered'/>
      <enumeration value='Unordered'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='Class' type='string' use='optional'/>
   <attribute name='NumberingType' use='optional'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='Decimal'/>
      <enumeration value='Roman'/>
      <enumeration value='roman'/>
      <enumeration value='Alphabetic'/>
      <enumeration value='alphabetic'/>
      <enumeration value='Number'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='StartValue' type='string' use='optional'/>
  </complexType>
 </element>

 <!-- ListItem -->
 <element name='ListItem'>
  <complexType>
   <sequence>
    <element ref='t:PageContent' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='PCID' type='string' use='optional'/>
  </complexType>
 </element>

 <!-- SimpleBulletList -->
 <element name='SimpleBulletList'>
  <complexType>
   <sequence>
    <element ref='t:SimpleListItem' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
  </complexType>
 </element>

 <!-- SimpleNumberedList -->
 <element name='SimpleNumberedList'>
  <complexType>
   <sequence>
    <element ref='t:SimpleListItem' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
  </complexType>
 </element>

 <!-- SimpleListItem -->
 <element name='SimpleListItem' type='t:Text' />

 <!-- FileList -->
 <element name='FileList'>
  <complexType>
   <sequence>
    <element ref='t:Title' minOccurs='0' maxOccurs='1'/>
    <element ref='t:FileItem' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
  </complexType>
 </element>

 <!-- FileItem -->
 <element name='FileItem'>
  <complexType>
   <sequence>
    <element ref='t:Identifier'/>
    <element ref='t:Location'/>
    <element ref='t:Format' minOccurs='0' maxOccurs='1'/>
   </sequence>
   <attribute name='PCID' type='string' use='optional'/>
   <attribute name='Class' type='string' use='optional'/>
  </complexType>
 </element>

 <!-- Table -->
 <element name='Table'>
  <complexType>
   <sequence>
    <element ref='t:Title' minOccurs='0' maxOccurs='1'/>
    <element ref='t:Caption' minOccurs='0' maxOccurs='1'/>
    <element ref='t:Summary' minOccurs='0' maxOccurs='1'/>
    <element ref='t:TableRow' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='Language' type='t:LanguageCode' use='required' />
   <attribute name='Id' type='ID' use='optional'/>
   <attribute name='Width' type='string' use='optional'/>
   <attribute name='Border' type='string' use='optional'/>
   <attribute name='HorizontalAlign' use='optional'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='Left'/>
      <enumeration value='Center'/>
      <enumeration value='Right'/>
      <enumeration value='LeftFloat'/>
      <enumeration value='RightFloat'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='CellPadding' type='string' use='optional'/>
   <attribute name='CellSpacing' type='string' use='optional'/>
   <attribute name='Class' type='string' use='optional'/>
   <attribute name='DataTable' use='optional'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='y'/>
      <enumeration value='n'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='Template' type='string' use='optional'/>
   <attribute name='HeaderRows' type='string' use='optional'/>
   <attribute name='HeaderCols' type='string' use='optional'/>
   <attribute name='FooterRows' type='string' use='optional'/>
   <attribute name='FooterCols' type='string' use='optional'/>
  </complexType>
 </element>

 <!-- Caption -->
 <element name='Caption'>
  <complexType mixed='true'>
   <complexContent>
    <extension base="t:TitleText">
	   <attribute name='Align' use='required'>
		<simpleType>
		 <restriction base='string'>
		  <enumeration value='top'/>
		  <enumeration value='bottom'/>
		 </restriction>
		</simpleType>
	   </attribute>
    </extension>
   </complexContent>
  </complexType>
 </element>

 <!-- Summary -->
 <element name='Summary' type='t:SimpleText' />

 <!-- TableRow -->
 <element name='TableRow'>
  <complexType>
   <sequence>
    <element ref='t:TableData' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='PCID' type='string' use='optional'/>
  </complexType>
 </element>

 <!-- TableData -->
 <element name='TableData'>
  <complexType>
   <sequence>
    <element ref='t:PageContent' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='Width' type='string' use='optional'/>
   <attribute name='Class' type='string' use='optional'/>
   <attribute name='PCID' type='string' use='optional'/>
   <attribute name='ColSpan' type='string' use='optional'/>
   <attribute name='RowSpan' type='string' use='optional'/>
   <attribute name='HorizontalAlign' use='optional'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='Left'/>
      <enumeration value='Center'/>
      <enumeration value='Right'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='Hidden' use='optional'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='Y'/>
      <enumeration value='N'/>
     </restriction>
    </simpleType>
   </attribute>
  </complexType>
 </element>

 <!-- ExtLink -->
 <element name='ExtLink'>
  <complexType mixed='true'>
   <complexContent>
    <extension base="t:LinkText">
	   <attribute name='Href' type='string' use='required'/>
	   <attribute name='Title' type='string' use='optional'/>
	   <attribute name='TargetFrame' use='optional'>
		<simpleType>
		 <restriction base='string'>
		  <enumeration value='Media'/>
		  <enumeration value='FAQ'/>
		  <enumeration value='Glossary'/>
		  <enumeration value='New'/>
		 </restriction>
		</simpleType>
	   </attribute>
    </extension>
   </complexContent>
  </complexType>
 </element>

 <!-- IntLink -->
 <element name='IntLink'>
  <complexType mixed='true'>
   <complexContent>
    <extension base="t:LinkText">
	   <attribute name='Target' type='string' use='required'/>
	   <attribute name='Type' use='required'>
		<simpleType>
		 <restriction base='string'>
		  <enumeration value='StructureObject'/>
		  <enumeration value='PageObject'/>
		  <enumeration value='MediaObject'/>
		  <enumeration value='GlossaryItem'/>
		  <enumeration value='RepositoryItem'/>
		  <enumeration value='File'/>
		 </restriction>
		</simpleType>
	   </attribute>
	   <attribute name='TargetFrame' use='optional'>
		<simpleType>
		 <restriction base='string'>
		  <enumeration value='Media'/>
		  <enumeration value='FAQ'/>
		  <enumeration value='Glossary'/>
		  <enumeration value='New'/>
		 </restriction>
		</simpleType>
	   </attribute>
	   <attribute name='Anchor' type='string' use='optional'/>
    </extension>
   </complexContent>
  </complexType>
 </element>

