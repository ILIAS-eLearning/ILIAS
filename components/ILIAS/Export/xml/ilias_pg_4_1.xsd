	<choice minOccurs='0' maxOccurs='unbounded'>
		<element ref='t:br'/>
		<element ref='t:Comment'/>
		<element ref='t:PageTurn'/>
		<element ref='t:Footnote'/>
		<element ref='t:Emph'/>
