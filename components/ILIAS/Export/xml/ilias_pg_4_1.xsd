 <!-- BibItem -->
 <element name='BibItem'>
  <complexType>
   <sequence>
    <element ref='t:Identifier'/>
    <element ref='t:Author' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:Booktitle'/>
    <element ref='t:CrossRef' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:Edition'/>
    <element ref='t:Editor' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:HowPublished'/>
    <element ref='t:WherePublished' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:Institution' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:Journal' minOccurs='0' maxOccurs='1'/>
    <element ref='t:Month' minOccurs='0' maxOccurs='1'/>
    <element ref='t:Pages' minOccurs='0' maxOccurs='1'/>
    <element ref='t:Publisher'/>
    <element ref='t:School' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:Series' minOccurs='0' maxOccurs='1'/>
    <element ref='t:Year'/>
    <choice>
     <element ref='t:URL'/>
     <sequence>
      <element ref='t:ISSN' minOccurs='0' maxOccurs='1'/>
      <element ref='t:ISBN' minOccurs='0' maxOccurs='1'/>
     </sequence>
    </choice>
   </sequence>
   <attribute name='Type' use='required'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='book'/>
      <enumeration value='inbook'/>
      <enumeration value='journalarticle'/>
      <enumeration value='proceedings'/>
      <enumeration value='inproceedings'/>
      <enumeration value='dissertation'/>
      <enumeration value='phdthesis'/>
      <enumeration value='mastersthesis'/>
      <enumeration value='techreport'/>
      <enumeration value='manual'/>
      <enumeration value='newspaperarticle'/>
      <enumeration value='av'/>
      <enumeration value='internet'/>
      <enumeration value='unpublished'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='Label' type='ID' use='required'/>
  </complexType>
 </element>

