 <!-- Tabs -->
 <element name='Tabs'>
  <complexType>
   <sequence>
    <element ref='t:Tab' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='Type' use='required'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='HorizontalAccordion'/>
      <enumeration value='VerticalAccordion'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='HorizontalAlign' use='optional'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='Left'/>
      <enumeration value='Center'/>
      <enumeration value='Right'/>
      <enumeration value='LeftFloat'/>
      <enumeration value='RightFloat'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='Behavior' use='optional'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='AllClosed'/>
      <enumeration value='FirstOpen'/>
      <enumeration value='ForceAllOpen'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='ContentWidth' type='string' use='optional'/>
   <attribute name='ContentHeight' type='string' use='optional'/>
   <attribute name='Template' type='string' use='optional'/>
  </complexType>
 </element>

