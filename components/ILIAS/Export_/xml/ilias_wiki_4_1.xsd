<?xml version="1.0" encoding="UTF-8"?>

<schema xmlns = 'http://www.w3.org/2001/XMLSchema'
	targetNamespace = "http://www.ilias.de/Modules/Wiki/wiki/4_1"
	xmlns:t = 'http://www.ilias.de/Modules/Wiki/wiki/4_1'>

	<!-- t:Data type -->
	<complexType name='Data' mixed='true'>
	</complexType>


	<!-- Mob -->
	<element name='Wiki'>
		<complexType>
		<sequence>
			<element ref='t:Id' minOccurs='1' maxOccurs='1'/>
			<element ref='t:Title' minOccurs='1' maxOccurs='1'/>
			<element ref='t:Description' minOccurs='1' maxOccurs='1'/>
			<element ref='t:StartPage' minOccurs='1' maxOccurs='1'/>
			<element ref='t:Short' minOccurs='1' maxOccurs='1'/>
			<element ref='t:Rating' minOccurs='1' maxOccurs='1'/>
			<element ref='t:Introduction' minOccurs='1' maxOccurs='1'/>
		</sequence>
		</complexType>
	</element>

	<element name='Id' type='t:Data' />
	<element name='Title' type='t:Data' />
	<element name='Description' type='t:Data' />
	<element name='StartPage' type='t:Data' />
	<element name='Short' type='t:Data' />
	<element name='Introduction' type='t:Data' />
	<element name='Rating' type='t:Data' />

	<!-- Wpg -->
	<element name='Wpg'>
		<complexType>
		<sequence>
			<element ref='t:Id' minOccurs='1' maxOccurs='1'/>
			<element ref='t:Title' minOccurs='1' maxOccurs='1'/>
			<element ref='t:WikiId' minOccurs='1' maxOccurs='1'/>
		</sequence>
		</complexType>
	</element>

	<element name='Id' type='t:Data' />
	<element name='Title' type='t:Data' />
	<element name='WikiId' type='t:Data' />

</schema>