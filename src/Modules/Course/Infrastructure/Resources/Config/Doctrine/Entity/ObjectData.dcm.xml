<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ObjectData" table="object_data">
    <indexes>
      <index name="i1_idx" columns="type"/>
      <index name="i4_idx" columns="import_id"/>
      <index name="i2_idx" columns="title"/>
    </indexes>
    <id name="objId" type="integer" column="obj_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="type" length="4" nullable="true">
      <options>
        <option name="fixed">1</option>
        <option name="default">none</option>
      </options>
    </field>
    <field name="title" type="string" column="title" length="255" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="description" type="string" column="description" length="128" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="owner" type="integer" column="owner" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="createDate" type="datetime" column="create_date" nullable="true"/>
    <field name="lastUpdate" type="datetime" column="last_update" nullable="true"/>
    <field name="importId" type="string" column="import_id" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="offline" type="boolean" column="offline" nullable="true"/>
  </entity>
</doctrine-mapping>
