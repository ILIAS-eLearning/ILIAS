--TEST--
"include" tag accept variables
--TEMPLATE--
{% include "foo.twig" with {'foo': 'bar'} %}
{% include "foo.twig" with vars1 %}
{% include "foo.twig" with vars2 %}
--TEMPLATE(foo.twig)--
{{ foo }}
--DATA--
return ['vars1' => ['foo' => 'bar'], 'vars2' => new ArrayObject(['foo' => 'bar'])]
--EXPECT--
bar
bar
bar
