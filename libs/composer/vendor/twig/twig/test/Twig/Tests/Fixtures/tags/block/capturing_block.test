--TEST--
capturing "block" tag
--DEPRECATION--
The spaceless tag is deprecated since Twig 2.7, use the spaceless filter instead.
--TEMPLATE--
{% set foo %}{% block foo %}FOO{% endblock %}{% endset %}
{{ foo }}
{% spaceless %}
{% block bar %}
<h1>
    <b>Title</b>
</h1>
{% endblock %}
{% endspaceless %}

{{ block('bar') }}
--DATA--
return array()
--EXPECT--
FOO
<h1><b>Title</b></h1>
<h1>
    <b>Title</b>
</h1>
