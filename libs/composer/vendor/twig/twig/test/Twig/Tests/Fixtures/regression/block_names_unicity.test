--TEST--
Block names are unique per template
--TEMPLATE--
{% extends 'layout' %}
{% block content -%}
    {% apply title -%}
        second
    {% endapply %}
{% endblock %}
--TEMPLATE(layout)--
{% apply title -%}
    first
{% endapply %}
{% block content %}{% endblock %}
--DATA--
return []
--EXPECT--
First
Second
